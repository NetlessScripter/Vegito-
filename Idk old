local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer
local PurpleMax = ReplicatedStorage.Assets.VFX.Gojo.PurpleMax

local function doEmitCount(inst)
	for _, v in ipairs(inst:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(10)
		end
	end
end

local function toggleAllEmitters(inst, state)
	for _, v in ipairs(inst:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v.Enabled = state
		end
	end
end

local function Pulse(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		local pulse = PurpleMax.Start.Pulse:Clone()
		pulse.Parent = hrp
		doEmitCount(pulse)
		Debris:AddItem(pulse, 5)
	end
end

local function Clap(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		local clap = PurpleMax.Start.Clap:Clone()
		clap.Parent = hrp
		doEmitCount(clap)
		Debris:AddItem(clap, 5)
	end
end

local function Lightning(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		local arm = PurpleMax.Start.LightningArm:Clone()
		arm.Parent = hrp
		toggleAllEmitters(arm, true)
		Debris:AddItem(arm, 5)
		local root = PurpleMax.Start.LightningRoot:Clone()
		root.Parent = hrp
		toggleAllEmitters(root, true)
		Debris:AddItem(root, 5)
		task.delay(3, function()
			toggleAllEmitters(root, false)
			toggleAllEmitters(arm, false)
		end)
	end
end

local function StartSequence(character)
	Pulse(character)
end

local redOrb, blueOrb
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local function Orbs(character)
    local player = Players.LocalPlayer
    local hrp = character:WaitForChild("HumanoidRootPart")

    local blueSound = Instance.new("Sound")
    blueSound.Name = "BlueAppear"
    blueSound.SoundId = "rbxassetid://14296780312"
    blueSound.Volume = 1
    blueSound.Parent = hrp
    blueSound:Play()

    blueOrb = PurpleMax.Blue:Clone()
    blueOrb.Anchored = true
    blueOrb.CFrame = hrp.CFrame * CFrame.new(5, 0, -5)
    blueOrb.Parent = workspace
    doEmitCount(blueOrb)
    toggleAllEmitters(blueOrb, true)

    task.wait(0.3)

    local redSound = Instance.new("Sound")
    redSound.Name = "RedAppear"
    redSound.SoundId = "rbxassetid://14296780135"
    redSound.Volume = 1
    redSound.Parent = hrp
    redSound:Play()

    redOrb = PurpleMax.Red:Clone()
    redOrb.Anchored = true
    redOrb.CFrame = hrp.CFrame * CFrame.new(-5, 0, -5)
    redOrb.Parent = workspace
    doEmitCount(redOrb)
    toggleAllEmitters(redOrb, true)

    task.delay(0.6, function()
        if redOrb and redOrb.Parent and blueOrb and blueOrb.Parent then
            local targetRed = hrp.CFrame * CFrame.new(-1, 0, -5)
            local targetBlue = hrp.CFrame * CFrame.new(1, 0, -5)
            TweenService:Create(redOrb, TweenInfo.new(1.4), {CFrame = targetRed}):Play()
            TweenService:Create(blueOrb, TweenInfo.new(1.4), {CFrame = targetBlue}):Play()
task.spawn(function()
wait(0.3)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://13072456071"
sound.Volume = 1
sound.Parent = hrp

sound:Play()
end)
        end
    end)
end

local function PurpleFade(character)
    local player = Players.LocalPlayer
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    if redOrb then redOrb:Destroy() redOrb = nil end
    if blueOrb then blueOrb:Destroy() blueOrb = nil end

    local purple = PurpleMax.PurpleSmall:Clone()
    purple.Anchored = true
    purple.CFrame = hrp.CFrame * CFrame.new(0, 0, -5)
    purple.Parent = workspace
    doEmitCount(purple)
    toggleAllEmitters(purple, true)

    if character == player.Character then
        local cc = Instance.new("ColorCorrectionEffect")
        cc.Parent = Lighting
        TweenService:Create(cc, TweenInfo.new(0.4), {
            Brightness = -0.3,
            Saturation = -0.4,
            TintColor = Color3.fromRGB(221, 180, 255),
        }):Play()

        local camera = workspace.CurrentCamera
        local tween1 = TweenService:Create(camera, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {FieldOfView = 60})
        local tween2 = TweenService:Create(camera, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {FieldOfView = 80})
        local tween3 = TweenService:Create(camera, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {FieldOfView = 70})
        tween1:Play()
        tween1.Completed:Connect(function()
            tween2:Play()
            tween2.Completed:Connect(function()
                tween3:Play()
            end)
        end)

        local test = ReplicatedStorage.Assets.VFX.Emotes.HollowPurple.PurplePart.ChargeUp:Clone()
        test.Parent = player.Character:WaitForChild("Right Arm")
        for _, child in ipairs(test:GetChildren()) do
            if child:IsA("ParticleEmitter") then
                child:Emit(5)
                child.Enabled = true
            end
        end
        task.spawn(function() task.wait(0.01) purple:Destroy() end)
        task.wait(0.8)

