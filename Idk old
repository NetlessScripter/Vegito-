    local function makeWindRing()
        local ring = l_v3339_0.WindRing:Clone()
        ring:SetPrimaryPartCFrame(ring.PrimaryPart.CFrame * CFrame.Angles(0, math.rad(math.random(0, 360)), 0))
        local start = ring.Start
        local goal = ring.End
        local mesh = start:Clone()
        mesh.Name = "ClonedMesh"
        mesh.Parent = l_v3339_0.CurrentTweens
        TweenService:Create(mesh.Decal, TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
            Transparency = 0.9
        }):Play()
        TweenService:Create(mesh, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = goal.Size,
            CFrame = goal.CFrame
        }):Play()
        TweenService:Create(mesh.Mesh, TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Scale = goal.Mesh.Scale
        }):Play()
        fastWait(0.2)
        TweenService:Create(mesh.Decal, TweenInfo.new(1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
            Transparency = 1
        }):Play()
        fastWait(1.2)
        mesh:Destroy()
    end

    local function makeSphere()
        local size = Random.new():NextNumber(24, 26)
        local sphere = l_SphereClone:Clone()
        sphere.Name = "Cloned"
        sphere.Parent = l_v3339_0.CurrentTweens
        TweenService:Create(sphere, TweenInfo.new(0.3, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
            Size = Vector3.new(size, size, size),
            Transparency = 3.65
        }):Play()
        fastWait(0.3)
        TweenService:Create(sphere, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            Size = Vector3.new(),
            Transparency = 1
        }):Play()
        fastWait(0.3)
        sphere:Destroy()
    end

    task.spawn(function()
        for _ = 1, 13 do
            task.spawn(makeSphere)
            fastWait(0.15)
        end
    end)

    for _ = 1, 13 do
        task.spawn(makeWindRing)
        fastWait(0.15)
    end
end)

