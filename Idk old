local ReplicatedStorage_upvr = game:GetService("ReplicatedStorage")
local Debris_upvr = game:GetService("Debris")
local RunService_upvr = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Gut_Rend_upvr = ReplicatedStorage_upvr.Assets.VFX.Minos["Gut Rend"]

local function getTweenData(obj, duration, typeName, style)
	local attribute = obj:GetAttribute(typeName.."_TweenParams")
	local defaults = {{"TweenStyle", style}, {"TweenDirection", Enum.EasingDirection.Out}}
	local result = {}
	if typeof(attribute) == "string" then
		local vals = {attribute:match("(%a+),(%a+)")}
		for i = 1, 2 do
			local key = defaults[i][1]
			if not vals[i] then
				result[key] = defaults[i][2]
			else
				local val = vals[i]
				if not pcall(function()
					result[key] = Enum[key][val]
				end) then
					result[key] = defaults[i][2]
				end
			end
		end
	else
		for _, v in defaults do
			result[v[1]] = v[2]
		end
	end
	return TweenInfo.new(duration, result.TweenStyle, result.TweenDirection)
end

local function MeshEmit(arg)
	local goal = arg.Parent:FindFirstChild("End")
	if not goal then return end
	local clone = arg:Clone()
	if clone:IsA("MeshPart") then
		clone.Transparency = tonumber(arg.Parent:GetAttribute("StartTransparency")) or 0
	elseif clone:IsA("BasePart") and clone:FindFirstChildOfClass("Decal") then
		clone.Transparency = 1
	end
	clone.Parent = workspace.CurrentCamera
	local duration = tonumber(arg.Parent:GetAttribute("Duration")) or 0.1
	if clone:FindFirstChildOfClass("Decal") then
		TweenService:Create(clone, getTweenData(arg.Parent, duration, "Part", Enum.EasingStyle.Cubic), {
			Size = goal.Size,
			CFrame = goal.CFrame
		}):Play()
	else
		TweenService:Create(clone, getTweenData(arg.Parent, duration, "Part", Enum.EasingStyle.Cubic), {
			Size = goal.Size,
			CFrame = goal.CFrame,
			Transparency = 1
		}):Play()
	end
	local smClone = clone:FindFirstChildOfClass("SpecialMesh")
	local smGoal = goal:FindFirstChildOfClass("SpecialMesh")
	if smClone and smGoal then
		TweenService:Create(smClone, getTweenData(arg.Parent, duration, "Mesh", Enum.EasingStyle.Sine), {
			Scale = smGoal.Scale
		}):Play()
	end
	local decalClone = clone:FindFirstChildOfClass("Decal")
	local decalGoal = goal:FindFirstChildOfClass("Decal")
	if decalClone and decalGoal then
		decalClone.Transparency = tonumber(arg.Parent:GetAttribute("StartTransparency")) or 0
		TweenService:Create(decalClone, getTweenData(arg.Parent, duration, "Decal", Enum.EasingStyle.Cubic), {
			Transparency = decalGoal.Transparency,
			Color3 = decalGoal.Color3
		}):Play()
	end
	task.delay(duration, function()
		clone:Destroy()
	end)
end

local function Velocity(arg1, arg2)
	local var9_upvr = arg1:FindFirstChild("HumanoidRootPart")
	if not var9_upvr or not arg2 or not arg2.Parent then return end
	local NumberValue_upvr = Instance.new("NumberValue")
	NumberValue_upvr.Value = 0
	arg2.Velocity = var9_upvr.CFrame.LookVector * NumberValue_upvr.Value
	local tween = TweenService:Create(NumberValue_upvr, TweenInfo.new(0.08333333333333333), {Value = 75})
	tween:Play()
	local conn
	conn = RunService_upvr.RenderStepped:Connect(function()
		arg2.Velocity = var9_upvr.CFrame.LookVector * NumberValue_upvr.Value
	end)
	tween.Completed:Once(function()
		task.wait(0.15)
		TweenService:Create(NumberValue_upvr, TweenInfo.new(0.5), {Value = 0}):Play()
		task.delay(0.5, function()
			arg2:Destroy()
			NumberValue_upvr:Destroy()
			conn:Disconnect()
		end)
	end)
end

local function Hitbox(arg1, arg2)
	if not arg1 or not arg2 or not arg1.Parent or not arg2.Parent then return end
	local HumanoidRootPart_upvr = arg1:FindFirstChild("HumanoidRootPart")
	if not HumanoidRootPart_upvr or not HumanoidRootPart_upvr.Parent then return end
	local params = OverlapParams.new()
	params.FilterType = Enum.RaycastFilterType.Exclude
	params.FilterDescendantsInstances = {arg1, workspace:WaitForChild("Map"), workspace:WaitForChild("VFX"), workspace:WaitForChild("Debris")}
	local conn
	conn = RunService_upvr.Heartbeat:Connect(function()
		conn:Disconnect()
	end)
end

local function doEmitCount(part)
	for _, emitter in ipairs(part:GetDescendants()) do
		if emitter:IsA("ParticleEmitter") then
			emitter:Emit(emitter:GetAttribute("EmitCount") or 1)
		end
	end
end

local function toggleAllEmitters(part, state)
	for _, emitter in ipairs(part:GetDescendants()) do
		if emitter:IsA("ParticleEmitter") then
			emitter.Enabled = state
		end
	end
end

local function StartVFX(arg1)
	local var25 = arg1:FindFirstChild("HumanoidRootPart")
	if not var25 then return end
	for _, v_upvr in ipairs(Gut_Rend_upvr.Startup:Clone():GetChildren()) do
		v_upvr.Parent = var25
		Debris_upvr:AddItem(v_upvr, 10)
		if v_upvr.Name == "fxGroundEmitAtStart" then
			doEmitCount(v_upvr)
		elseif v_upvr.Name ~= "fxGroundEmitAtEnd" then
			task.delay(0.4, function()
				doEmitCount(v_upvr)
			end)
		else
			toggleAllEmitters(v_upvr, true)
			task.delay(0.4, function()
				toggleAllEmitters(v_upvr, false)
			end)
		end
	end
end

local function EndVFX(arg1, arg2)
	local var32_upvr = arg1:FindFirstChild("HumanoidRootPart")
	local var33_upvr = arg2:FindFirstChild("Head")
	if not var32_upvr or not var33_upvr then return end
	local clone_2_upvr = Gut_Rend_upvr.User:Clone()
	local clone = Gut_Rend_upvr.Victim.fxHeadGrab:Clone()
	Debris_upvr:AddItem(clone, 3)
	clone.Parent = var33_upvr
	doEmitCount(clone)
	task.delay(0.31666666666666665, function()
		if var32_upvr.Parent and var33_upvr.Parent then
			local fxSlash = clone_2_upvr.fxSlash
			fxSlash.Parent = var32_upvr
			doEmitCount(fxSlash)
			Debris_upvr:AddItem(fxSlash, 3)
		end
	end)
	task.delay(0.9, function()
		if var32_upvr.Parent and var33_upvr.Parent then
			local fxSlash2 = clone_2_upvr.fxSlash2
			fxSlash2.Parent = var32_upvr
			doEmitCount(fxSlash2)
			Debris_upvr:AddItem(fxSlash2, 3)
		end
	end)
	task.delay(1.65, function()
		if var32_upvr.Parent and var33_upvr.Parent then
			local fxSlash3 = clone_2_upvr.fxSlash3
			fxSlash3.Parent = var32_upvr
			doEmitCount(fxSlash3)
			Debris_upvr:AddItem(fxSlash3, 3)
		end
	end)
	task.delay(1.7333333333333334, function()
		if var32_upvr.Parent and var33_upvr.Parent then
			local fxHit = clone_2_upvr.fxHit
			fxHit.Parent = var32_upvr
			doEmitCount(fxHit)
			Debris_upvr:AddItem(fxHit, 7)
			local clone_5 = Gut_Rend_upvr.LastHit.Mesh1:Clone()
			local clone_4 = Gut_Rend_upvr.LastHit.Mesh2:Clone()
			local clone_3 = Gut_Rend_upvr.LastHit.Mesh3:Clone()
			clone_5:PivotTo(var32_upvr.CFrame - Vector3.new(0, 3, 0))
			clone_4:PivotTo(var32_upvr.CFrame - Vector3.new(0, 3, 0))
			clone_3:PivotTo(var32_upvr.CFrame * CFrame.new(0, -2, 3))
			clone_3:PivotTo(var32_upvr.CFrame * CFrame.new(0, 0, 5))
			MeshEmit(clone_5.Start)
			MeshEmit(clone_4.Start)
			MeshEmit(clone_3.Start)
			MeshEmit(Gut_Rend_upvr.LastHit.Mesh4:Clone().Start)
		end
	end)
end
