				v_4:Emit(v_4:GetAttribute("EmitCount"))
			end
		end
		Start.Decal.Transparency = 0
		TweenService_upvr:Create(Start, TweenInfo.new(0.2, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Size = End_2.Size;
			Position = End_2.Position;
			Orientation = End_2.Orientation;
		}):Play()
		TweenService_upvr:Create(Start.Decal, TweenInfo.new(0.2, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Transparency = End_2.Decal.Transparency;
		}):Play()
