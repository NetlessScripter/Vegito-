local targetAnimID = "rbxassetid://12342141464"

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == targetAnimID then
        animationTrack:Stop()

task.spawn(function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local turnData = {}

local function playClapSound(part)
    local sound = Instance.new("Sound")
    sound.SoundId = ({
        "rbxassetid://15962163599", 
        "rbxassetid://15962163752", 
        "rbxassetid://15962163891", 
        "rbxassetid://15962164060"
    })[math.random(1, 4)]
    sound.Parent = part
    sound.PlaybackSpeed = 1
    sound.Volume = 0
    sound.RollOffMaxDistance = 100
    sound:Play()
end

local Keyframes = {
    clap = function(data)
        if not data.turn then
            data.turn = 0
        end
        data.turn = data.turn + 1
        local part = data.turn % 2 == 0 and character:FindFirstChild("Left Leg") or character:FindFirstChild("Right Leg")
        if part then
            playClapSound(part)
        end
    end
}

local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://15962326593"
local track = humanoid:LoadAnimation(anim)
track.Looped = true
track:Play()

local startTime = tick()
while tick() - startTime < 1.7 do
    task.wait(0.1)
    Keyframes.clap(turnData)
end

track:Stop()
end)
local function GetGitSound(GithubSnd, SoundName)
    if not isfile(SoundName .. ".mp3") then
        writefile(SoundName .. ".mp3", game:HttpGet(GithubSnd))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
    sound.Volume = 3
    sound.Name = SoundName
    sound.Parent = workspace
    return sound
end

local sound = GetGitSound("https://raw.githubusercontent.com/NetlessScripter/BEST-TSB-MOVESET/refs/heads/main/copy_F736F1C1-EE39-47E1-BD00-AD4BABEFA351.mp3", "SukunaAwakening2.0")
sound:Play()

local function GetGitSound(GithubSnd, SoundName)
    local url = GithubSnd
    if not isfile(SoundName .. ".mp3") then
        writefile(SoundName .. ".mp3", game:HttpGet(url))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
    sound.Volume = 2.5
    sound.Name = SoundName
    sound.Parent = workspace
    return sound
end

local sound = GetGitSound("https://raw.githubusercontent.com/NetlessScripter/BEST-TSB-MOVESET/refs/heads/main/copy_6E32F03D-D3CA-45D3-879F-E122D290F71A.mp3?raw=true", "SukunaUltTheme")
sound:Play()

-- Sukuna Awakening VFX
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Lighting = game:GetService("Lighting")
local Debris = game:GetService("Debris")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Atmosphere = Lighting:FindFirstChildOfClass("Atmosphere")
local Clouds = workspace.Terrain:FindFirstChildOfClass("Clouds")
local Assets = ReplicatedStorage:WaitForChild("Assets")

local OldColor = Atmosphere and Atmosphere.Color
local OldDecay = Atmosphere and Atmosphere.Decay
local OldDensity = Atmosphere and tonumber(Atmosphere.Density)
local OldHaze = Atmosphere and tonumber(Atmosphere.Haze)
local OldFOV = workspace.CurrentCamera.FieldOfView
local OldClockTime = Lighting.ClockTime
local OldCloudDensity = Clouds and Clouds.Density

local function VisualsStart(character)
	if character and character.Parent then
		local clone = Assets:WaitForChild("VFX"):WaitForChild("ultthukuna"):Clone()
		task.delay(1.67, function()
			if character and character.Parent then
				for _, v in pairs(character:GetDescendants()) do
					if v.Name ~= "HumanoidRootPart" and (v:IsA("BasePart") or v:IsA("Decal")) then
						v:SetAttribute("OldTransparency", v.Transparency)
						TweenService:Create(v, TweenInfo.new(0.15, Enum.EasingStyle.Linear), {Transparency = 1}):Play()
					end
				end
				local vfxFolder = workspace:FindFirstChild("VFX") or Instance.new("Folder", workspace)
				vfxFolder.Name = "VFX"
				clone.Parent = vfxFolder
				local hrp = character:FindFirstChild("HumanoidRootPart")
				if hrp then
					clone.CFrame = hrp.CFrame
				end
				for _, v in pairs(clone:GetDescendants()) do
					if v:IsA("ParticleEmitter") and not string.find(v.Name, "EMIT") then
						v.Enabled = true
					end
				end
				local conn
				conn = RunService.RenderStepped:Connect(function()
					if character and character.Parent and hrp and clone.Parent then
						clone.CFrame = hrp.CFrame
					else
						if conn then conn:Disconnect() end
					end
				end)
			end
		end)
		task.delay(3.46, function()
			if character and character.Parent then
				for _, v in pairs(character:GetDescendants()) do
					if v.Name ~= "HumanoidRootPart" and (v:IsA("BasePart") or v:IsA("Decal")) then
						TweenService:Create(v, TweenInfo.new(0.15, Enum.EasingStyle.Linear), {
							Transparency = v:GetAttribute("OldTransparency") or 0
						}):Play()
					end
				end
				Debris:AddItem(clone, 2)
				for _, v in pairs(clone:GetDescendants()) do
					if v:IsA("ParticleEmitter") then
						if string.find(v.Name, "EMIT") then
							v:Emit(v:GetAttribute("EmitCount") or 1)
						end
						v.Enabled = false
					end
				end
			end
		end)
	end
end

local function CloseVFX(character)
	if not Atmosphere or not Clouds then return end
	local hrp = character and character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local light = Instance.new("PointLight", hrp)
	light.Range = 0
	light.Brightness = 0
	light.Color = Color3.new(1, 0, 0)

	TweenService:Create(workspace.CurrentCamera, TweenInfo.new(2), {FieldOfView = 50}):Play()
	TweenService:Create(Atmosphere, TweenInfo.new(2), {
		Color = Color3.new(0, 0, 0),
		Decay = Color3.new(0, 0, 0),
		Density = 1,
		Haze = 10
	}):Play()
	TweenService:Create(light, TweenInfo.new(2), {Range = 20, Brightness = 0.5}):Play()
	TweenService:Create(Lighting, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ClockTime = 0}):Play()
	TweenService:Create(Clouds, TweenInfo.new(1), {Density = 0}):Play()

	task.delay(1.67, function()
		TweenService:Create(workspace.CurrentCamera, TweenInfo.new(0.5), {FieldOfView = 130}):Play()
		TweenService:Create(light, TweenInfo.new(2), {Range = 20}):Play()
		TweenService:Create(Atmosphere, TweenInfo.new(0.5), {
			Color = OldColor,
			Decay = OldDecay,
			Density = OldDensity,
			Haze = OldHaze
		}):Play()
		TweenService:Create(light, TweenInfo.new(0.5), {Range = 50, Brightness = 3}):Play()
	end)

	task.delay(3.46, function()
		TweenService:Create(workspace.CurrentCamera, TweenInfo.new(2), {FieldOfView = OldFOV}):Play()
		TweenService:Create(light, TweenInfo.new(2), {Range = 0, Brightness = 0}):Play()
		TweenService:Create(Lighting, TweenInfo.new(2), {ClockTime = OldClockTime}):Play()
		TweenService:Create(Clouds, TweenInfo.new(1), {Density = OldCloudDensity}):Play()
		Debris:AddItem(light, 2)
	end)
end

local function ClientStart(character)
	if character and character.Parent then
		local hrp = character:FindFirstChild("HumanoidRootPart")
		if hrp then
			local speedVal = Instance.new("NumberValue")
			speedVal.Value = 9.5
			local bv = Instance.new("BodyVelocity")
			bv.MaxForce = Vector3.new(100000, 0, 100000)
			bv.P = 25000
			bv.Velocity = hrp.CFrame.LookVector * speedVal.Value
			bv.Parent = hrp

			local conn
			conn = RunService.Heartbeat:Connect(function()
				if character and character.Parent and bv.Parent then
					bv.Velocity = hrp.CFrame.LookVector * speedVal.Value
				else
					if conn then conn:Disconnect() end
				end
			end)

			task.delay(1.67, function()
				if character and character.Parent and bv.Parent then
					speedVal.Value = 50
				end
			end)

			task.delay(3.46, function()
				if character and character.Parent and bv.Parent then
					local tween = TweenService:Create(speedVal, TweenInfo.new(0.85, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Value = 0})
					tween.Completed:Connect(function()
						tween:Destroy()
						speedVal:Destroy()
						if conn then conn:Disconnect() end
						if bv.Parent then bv:Destroy() end
					end)
					tween:Play()
				end
			end)
		end
	end
end
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
VisualsStart(character)
CloseVFX(character)
ClientStart(character)

    end
end

