local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Assets = ReplicatedStorage:WaitForChild("Assets")
local DivergantFinisher = Assets.VFX.Yuji:WaitForChild("DivergantFinisher")

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local function doEmitCount(parent)
	for _, v in pairs(parent:GetChildren()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 1)
		end
	end
end

local function enableForTime(parent, time)
	for _, v in pairs(parent:GetChildren()) do
		if v:IsA("ParticleEmitter") then
			v.Enabled = true
		end
	end
	task.delay(time, function()
		for _, v in pairs(parent:GetChildren()) do
			if v:IsA("ParticleEmitter") then
				v.Enabled = false
			end
		end
	end)
end

local function Hit(character, type_)
	if character.Parent then
		type_ = type_ or "Hit"
		local root = character:WaitForChild("HumanoidRootPart")
		local vfx

		if type_ == "Hit" then
			vfx = Assets.VFX.DivergentFist.divergentFist:Clone()
		elseif type_ == "Miss" then
			vfx = Assets.VFX.DivergentFist.divergentFistfail:Clone()
		elseif type_ == "Miss_flash" then
			vfx = Assets.VFX.DivergentFist.BlackFlash:Clone()
			vfx.CFrame = CFrame.new(0, 0, -1.5)
		elseif type_ == "flash_attacker" then
			vfx = Assets.VFX.DivergentFist.BlackFlash2:Clone()
			vfx.CFrame = CFrame.new(0, 0, -1.5)
			for _, v in pairs(Assets.Effects.BlackFlash:Clone():GetChildren()) do
				v.Parent = game.Lighting
				game.Debris:AddItem(v, 0.09)
			end
		elseif type_ == "flash_victim" then
			vfx = Assets.VFX.DivergentFist.BlackFlash:Clone()
			vfx.CFrame = CFrame.new(0, 0, 0)
			for _, v in pairs(Assets.Effects.BlackFlash:Clone():GetChildren()) do
				v.Parent = game.Lighting
				game.Debris:AddItem(v, 0.09)
			end
		end

		if vfx then
			vfx.Parent = root
			game.Debris:AddItem(vfx, 5)
			doEmitCount(vfx)
		end
	end
end

local function Snow(character)
	local root = character:FindFirstChild("HumanoidRootPart")
	task.spawn(function()
		local bf = DivergantFinisher.fxAttach.fxBfEm:Clone()
		bf.Parent = root
		doEmitCount(bf)
		task.delay(3, bf.Destroy, bf)

		local en = DivergantFinisher.fxAttach.fxBfEn:Clone()
		en.Parent = root
		enableForTime(bf, 0.3)
		task.delay(1, en.Destroy, en)
	end)
end

local function TargetGround1(character)
	local clone = DivergantFinisher.fxAttach.fxTargetGr1:Clone()
	clone.Parent = character:FindFirstChild("HumanoidRootPart")
	doEmitCount(clone)
	task.delay(6, clone.Destroy, clone)
end

local function TargetGround2(character)
	local clone = DivergantFinisher.fxAttach.fxTargetGr2:Clone()
	clone.Parent = character:FindFirstChild("HumanoidRootPart")
	doEmitCount(clone)
	task.delay(6, clone.Destroy, clone)
end

local function AuraF(character)
	local clone = DivergantFinisher.fxAttach.fxAuraFarming:Clone()
	clone.Parent = character:WaitForChild("Torso")
	enableForTime(clone, 4)
	task.delay(5, clone.Destroy, clone)
end

local function Afterimage1(character)
