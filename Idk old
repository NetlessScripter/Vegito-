local function setup(character)
    character:WaitForChild("Humanoid").AnimationPlayed:Connect(onAnimationPlayed)
end

if player.Character then
    setup(player.Character)
end

player.CharacterAdded:Connect(setup)

local player = game.Players.LocalPlayer
local soundsIds = {
    "rbxassetid://18435468901",
    "rbxassetid://129080118003311"
}
local activeSounds = {}
local activeTracks = {}
local function stopAll()
    for _, sound in ipairs(activeSounds) do
        if sound then
            sound:Stop()
            sound:Destroy()
        end
    end
    activeSounds = {}
    for _, track in ipairs(activeTracks) do
        if track then
            track:Stop()
        end
    end
    activeTracks = {}
end
local function playSound(id, parent)
    local sound = Instance.new("Sound")
    sound.SoundId = id
    sound.Volume = 1.5
    sound.Parent = parent
    sound:Play()
    table.insert(activeSounds, sound)
    sound.Ended:Connect(function()
        sound:Destroy()
    end)
end
local function onRespawn(character)
    if player:GetAttribute("Character") ~= "Bald" then return end

    local humanoid = character:WaitForChild("Humanoid")
    local root = character:WaitForChild("HumanoidRootPart")
    for _, soundId in ipairs(soundsIds) do
        playSound(soundId, root)
    end
    for _, track in ipairs(humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
        table.insert(activeTracks, track)
    end
end
player.CharacterAdded:Connect(onRespawn)

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function onCharacterAdded(character)
    if character:GetAttribute("Character") == "Bald" then

local TweenService = game:GetService("TweenService")
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local highlight = Instance.new("Highlight")
highlight.Name = "Highlight"
highlight.FillColor = Color3.fromRGB(255, 255, 255)
highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
highlight.FillTransparency = 0
highlight.OutlineTransparency = 0
highlight.DepthMode = Enum.HighlightDepthMode.Occluded
highlight.Parent = Character

local conn
conn = Character.ChildAdded:Connect(function(child)
    if child:IsA("Highlight") and child ~= highlight then
        highlight:Destroy()
        conn:Disconnect()
    end
end)

task.wait()

if highlight.Parent then
    local tweenInfo = TweenInfo.new(2.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
    local goal = {
        FillTransparency = 1,
        OutlineTransparency = 1
    }
    TweenService:Create(highlight, tweenInfo, goal):Play()
end
workspace.Thrown.ChildAdded:Connect(function(d)
    if d.Name == "Ring" or d.Name == "Model" then
        game:GetService("RunService").Heartbeat:Connect(function()
            d:Destroy()
        end)
    end
end)

game.Players.LocalPlayer.Character.ChildAdded:Connect(function(d)
    if d.Name == "BarrageBind" then
        game:GetService("RunService").Heartbeat:Connect(function()
            d:Destroy()
        end)
    end
end)
    end
end

player.CharacterAdded:Connect(onCharacterAdded)

