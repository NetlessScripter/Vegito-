s.SoundId = g(f)
s.Volume = 0.4
s.Parent = workspace
s:Play()

task.spawn(function()
    wait(40)
    local duration = 1.7
    local startVol = s.Volume
    local t = 0
    while t < duration do
        t = t + game:GetService("RunService").RenderStepped:Wait()
        s.Volume = startVol * (1 - t/duration)
    end
    s:Stop()
    s:Destroy()
end)
end)
