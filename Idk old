local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local TweenService = game:GetService("TweenService")
local LapseBlueRework = ReplicatedStorage:WaitForChild("Assets").VFX.LapseBlueRework

local function toggleAllEmitters(parent, enabled)
	for _, v in parent:GetDescendants() do
		if v:IsA("ParticleEmitter") then
			v.Enabled = enabled
		end
	end
end

local function PullStart(character)
	if not character or not character.Parent then return end
	local RightArm = character:FindFirstChild("Right Arm")
	local HRP = character:FindFirstChild("HumanoidRootPart")
	if not HRP or not RightArm then return end
	local stopped = false
	local trailClone = LapseBlueRework.HandTrail:Clone()
	for _, v in trailClone:GetChildren() do
		v.Parent = RightArm
		Debris:AddItem(v, 0.485)
	end
	local attachClone = LapseBlueRework.HandPull.Attachment:Clone()
	attachClone.Parent = RightArm
	Debris:AddItem(attachClone, 2)
	task.delay(0.175, function()
		if not stopped and attachClone.Parent then
			toggleAllEmitters(attachClone, true)
		end
	end)
	task.delay(0.472, function()
		if not stopped and attachClone.Parent then
			toggleAllEmitters(attachClone, false)
		end
	end)
end

local function pulldebris(cf)
	if not cf then return end
	local params = RaycastParams.new()
	params.FilterType = Enum.RaycastFilterType.Include
	params.FilterDescendantsInstances = {workspace.Map.Parts}
	local ray = workspace:Raycast(cf.Position + Vector3.new(0,4,0), Vector3.new(0,-8,0), params)
	if ray then
		for i = 1, 9 do
			local size = math.random(1,2)
			local part = Instance.new("Part")
			part.Parent = workspace.Debris
			part.CanCollide = false
			part.CanTouch = false
			part.CanQuery = false
			if ray.Material == Enum.Material.Grass then
				part.Material = Enum.Material.Ground
				part.Color = Color3.fromRGB(62,47,34)
			else
				part.Material = ray.Material
				part.Color = ray.Instance.Color
			end
			part.CollisionGroup = "Rocks"
			part.Size = Vector3.new(size, size, size)
			part.CFrame = cf * CFrame.new(math.random(-6,6), -2.5, -(10+i)) * CFrame.Angles(math.rad(math.random(0,360)), math.rad(math.random(0,360)), math.rad(math.random(0,360)))
			local dir = part.Position - cf.Position
			local bv = Instance.new("BodyVelocity")
			bv.Parent = part
			bv.MaxForce = Vector3.new(1,1,1)*math.huge
			bv.P = math.huge
			bv.Velocity = dir.Unit * -(dir.Magnitude*0.5) + Vector3.new(0,5,0)
			Debris:AddItem(bv, 1.45)
			local rnd = Random.new(os.clock())
			task.delay(1.45, function()
				if bv.Parent then bv:Destroy() end
				part.CanCollide = true
				local bv2 = Instance.new("BodyVelocity")
				bv2.Parent = part
				bv2.MaxForce = Vector3.new(1,1,1)*math.huge
				bv2.P = math.huge
				bv2.Velocity = cf.Position.Unit + Vector3.new(rnd:NextInteger(-40,40), 12, rnd:NextInteger(-40,40))
				Debris:AddItem(bv2, 0.075)
				task.delay(rnd:NextNumber(9,10), function()
					TweenService:Create(part, TweenInfo.new(3), {Transparency=1, Size=Vector3.new(0.001,0.001,0.001)}):Play()
					Debris:AddItem(part, 4)
				end)
			end)
			task.delay(0.55, function()
				bv.Velocity = Vector3.new(0,3,0)
			end)
		end
	end
end

local function PullSuccess(arg1, arg2)
	if not arg1 or not arg2 or not arg1.Parent then return end
	local beamClone = LapseBlueRework.gojobeam:Clone()
	beamClone.Parent = workspace.VFX
	for _, v in beamClone:GetDescendants() do
		if v:IsA("Beam") then
			TweenService:Create(v, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Width0=0, Width1=0, TextureSpeed=-1}):Play()
		end
	end
	beamClone.CFrame = arg2 * CFrame.new(1,0,-4)
	Debris:AddItem(beamClone, 3.25)
	local bringBack = LapseBlueRework.gojobringback:Clone()
	bringBack.Parent = workspace.VFX
	bringBack.CFrame = arg2 * CFrame.new(0,0,-25) * CFrame.Angles(math.pi/2,0,0)
	Debris:AddItem(bringBack,4)
	local back2 = LapseBlueRework.gojoback2:Clone()
	back2.Parent = workspace.VFX
	back2.CFrame = arg2 * CFrame.new(0,0,-25) * CFrame.Angles(0,0,math.pi)
	Debris:AddItem(back2,4)
	for _, v in {bringBack, back2} do
		for _, p in ipairs(v:GetDescendants()) do
			if p:IsA("ParticleEmitter") then
				p:Emit(1)
			end
		end
	end
	for _, v in beamClone.Dash:GetChildren() do
		if v:IsA("Beam") then v.Enabled = true end
	end
	task.wait(1)
	for _, v in beamClone.Dash:GetChildren() do
		if v:IsA("Beam") then v.Enabled = false end
	end
end
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

PullStart(character)
wait(1)
PullSuccess(character, hrp.CFrame)

end)
end

