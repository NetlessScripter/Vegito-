local bv = Instance.new("BodyVelocity")
bv.MaxForce = Vector3.new(1e5, 0, 1e5)
bv.Velocity = Vector3.new(0,0,0)
bv.Parent = hrp

local speed = 50
local duration = 4.7
local startTime = tick()
local heartbeat = game:GetService("RunService").Heartbeat
local conn

local function updateVelocity()
	local camLook = camera.CFrame.LookVector
	local flatLook = Vector3.new(camLook.X, 0, camLook.Z)
	if flatLook.Magnitude > 0 then
		flatLook = flatLook.Unit
	end
	bv.Velocity = flatLook * speed
	hrp.CFrame = CFrame.new(hrp.Position, hrp.Position + flatLook)
end

conn = heartbeat:Connect(function()
	updateVelocity()
	if tick() - startTime >= duration then
		bv:Destroy()
		conn:Disconnect()
	end
end)

VisualsStart(character)
Breath1(character)
Breath2(character)
GroundSmoke(character)
Flashstep(character)
Flashstep2(character)
Run(character)

	speed = speed + 100
	updateVelocity()

local TweenService = game:GetService("TweenService")
local cam = workspace.CurrentCamera
local tween = TweenService:Create(cam, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {FieldOfView = 80})
tween:Play()

