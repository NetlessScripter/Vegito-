local Assets_upvr = ReplicatedStorage:WaitForChild("Assets")
local Limitless_Combo_upvr = Assets_upvr.VFX.Gojo["Limitless Combo"]
local Meshes_upvr = Limitless_Combo_upvr.Meshes

local function Trail(arg1, arg2)
	if arg2 == 3 and arg1:FindFirstChild("Left Leg") then
		local clone_34 = Assets_upvr.VFX.LimitlessCombo.Trail3:Clone()
		clone_34.Parent = arg1["Left Leg"]
		Debris_upvr:AddItem(clone_34, 0.7)
		for _, v_upvr in pairs(clone_34:GetDescendants()) do
			if v_upvr:IsA("Trail") then
				v_upvr.Enabled = true
				task.delay(0.55, function()
					v_upvr.Enabled = false
				end)
			end
		end
	end
end

local function Hits(arg1, arg2)
	if not arg1 or not arg2 then return end
	if not arg1.Parent then return end
	local HumanoidRootPart_2 = arg1:FindFirstChild("HumanoidRootPart")
	if not HumanoidRootPart_2 then return end

	if arg2 == 1 then
		local clone_26 = Assets_upvr.VFX.LimitlessCombo.Presets.Preset0:Clone()
		Debris_upvr:AddItem(clone_26, 2.75)
		clone_26.Parent = workspace:WaitForChild("VFX")
		clone_26:PivotTo(HumanoidRootPart_2.CFrame)
		local Start_4 = clone_26.Start
		Start_4.Parent = workspace:WaitForChild("VFX")
		local End_4 = clone_26.End
		End_4.Parent = workspace:WaitForChild("VFX")
		local clone_21 = Assets_upvr.VFX.LimitlessCombo.HitPart:Clone()
		clone_21.Parent = workspace:WaitForChild("VFX")
		clone_21.CFrame = CFrame.new(HumanoidRootPart_2.CFrame * CFrame.new(0, 0, -4).Position, HumanoidRootPart_2.Position)
		Debris_upvr:AddItem(clone_21, 1)
		for _, v_2 in pairs(clone_21.FacePunch:GetDescendants()) do
			if v_2:IsA("ParticleEmitter") then
				v_2:Emit(v_2:GetAttribute("EmitCount"))
			end
		end
		Start_4.Decal.Transparency = 0
		TweenService_upvr:Create(Start_4, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Size = End_4.Size;
			Position = End_4.Position;
			Orientation = End_4.Orientation;
		}):Play()
		TweenService_upvr:Create(Start_4.Decal, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Transparency = End_4.Decal.Transparency;
		}):Play()
		TweenService_upvr:Create(Start_4.Mesh, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Scale = End_4.Mesh.Scale;
		}):Play()
		task.wait(0.1)
		clone_26:Destroy()
		End_4:Destroy()
		Start_4:Destroy()
	elseif arg2 == 2 then
		local clone_33 = Assets_upvr.VFX.LimitlessCombo.Presets.Preset1:Clone()
		Debris_upvr:AddItem(clone_33, 2.75)
		clone_33.Parent = workspace:WaitForChild("VFX")
		clone_33:PivotTo(HumanoidRootPart_2.CFrame)
		local Start_5 = clone_33.Start
		Start_5.Parent = workspace:WaitForChild("VFX")
		local End_5 = clone_33.End
		End_5.Parent = workspace:WaitForChild("VFX")
		local clone_13 = Assets_upvr.VFX.LimitlessCombo.HitPart:Clone()
		clone_13.Parent = workspace:WaitForChild("VFX")
		clone_13.CFrame = CFrame.new(HumanoidRootPart_2.CFrame * CFrame.new(0, 0, -4).Position, HumanoidRootPart_2.Position)
		Debris_upvr:AddItem(clone_13, 1)
		for _, v_3 in pairs(clone_13.Attachment:GetDescendants()) do
			if v_3:IsA("ParticleEmitter") then
				v_3:Emit(v_3:GetAttribute("EmitCount"))
			end
		end
		Start_5.Decal.Transparency = 0
		TweenService_upvr:Create(Start_5, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Size = End_5.Size;
			Position = End_5.Position;
			Orientation = End_5.Orientation;
		}):Play()
		TweenService_upvr:Create(Start_5.Decal, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Transparency = End_5.Decal.Transparency;
		}):Play()
		TweenService_upvr:Create(Start_5.Mesh, TweenInfo.new(0.1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Scale = End_5.Mesh.Scale;
		}):Play()
		task.wait(0.1)
		End_5:Destroy()
		Start_5:Destroy()
	elseif arg2 == 3 then
		local clone_5 = Assets_upvr.VFX.LimitlessCombo.Presets.Preset2:Clone()
		Debris_upvr:AddItem(clone_5, 2.75)
		clone_5.Parent = workspace:WaitForChild("VFX")
		clone_5:PivotTo(HumanoidRootPart_2.CFrame)
		local Start = clone_5.Start
		Start.Parent = workspace:WaitForChild("VFX")
		local End_2 = clone_5.End
		End_2.Parent = workspace:WaitForChild("VFX")
		local clone = Assets_upvr.VFX.LimitlessCombo.HitUp:Clone()
		clone.Parent = workspace:WaitForChild("VFX")
		clone.CFrame = CFrame.new(HumanoidRootPart_2.CFrame * CFrame.new(0, -0.5, -4).Position, HumanoidRootPart_2.Position)
		Debris_upvr:AddItem(clone, 1)
