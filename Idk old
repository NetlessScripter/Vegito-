local AllowedUsers = {}

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function IsAllowed(player)
    for _, name in ipairs(AllowedUsers) do
        if player.Name == name then
            return true
        end
    end
    return false
end

