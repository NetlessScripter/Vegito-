local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Debris = game:GetService("Debris")
local LocalPlayer = game.Players.LocalPlayer
local Assets = ReplicatedStorage:WaitForChild("Assets")
local NewUlt = Assets.VFX.Gojo.NewUlt
local AttachmentHolder = NewUlt:WaitForChild("AttachmentHolder")

local function doEmitCount(parent)
	for _, v in pairs(parent:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 1)
		end
	end
end

local function VisualsStart(char)
	if not char or not char.Parent then return end
	local HRP = char:FindFirstChild("HumanoidRootPart")
	local Torso = char:FindFirstChild("Torso")
	if not HRP or not Torso then return end
	local clone = Assets.VFX.GojoUlt:Clone()
	Debris:AddItem(clone, 9.666)
	for _, v in pairs(clone:GetDescendants()) do
		Debris:AddItem(v, 9.666)
	end
	local HitGround = clone:FindFirstChild("HitGround")
	local Up = clone:FindFirstChild("Up")
	if HitGround then
		HitGround.Parent = workspace:WaitForChild("VFX")
		HitGround.CFrame = HRP.CFrame * CFrame.new(0, -0.7, 0)
	end
	if Up then
		Up.Parent = workspace:WaitForChild("VFX")
		Up.CFrame = HRP.CFrame * CFrame.new(0, -0.7, 0)
	end
	if clone:FindFirstChild("Torso") then
		for _, v in pairs(clone.Torso:GetChildren()) do
			v.Parent = Torso
		end
		local fly = clone.Torso:FindFirstChild("fly")
		if fly then
			task.delay(0.3, function()
				for _, p in pairs(fly:GetDescendants()) do
					if p:IsA("ParticleEmitter") then
						p.Enabled = true
						task.delay(5, function()
							p.Enabled = false
						end)
					end
				end
				if Up then doEmitCount(Up) end
			end)
		end
		local honored = clone.Torso:FindFirstChild("honoredgyat")
		if honored and honored:FindFirstChild("Parent") and honored.Parent:FindFirstChild("stars") then
			task.delay(3.35, function()
				for _, p in pairs(honored:GetDescendants()) do
					if p:IsA("ParticleEmitter") then
						p.Enabled = true
						honored.Parent.stars.Enabled = true
						task.delay(2.5, function()
							p.Enabled = false
							honored.Parent.stars.Enabled = false
						end)
					end
				end
			end)
		end
	end
	if clone:FindFirstChild("Root") then
		for _, v in pairs(clone.Root:GetChildren()) do
			v.Parent = HRP
		end
	end
end

local function Breath1(char)
	local head = char:FindFirstChild("Head")
	if not head then return end
	local b1 = AttachmentHolder:FindFirstChild("Breath1")
	if b1 then
		local b1c = b1:Clone()
		b1c.Parent = head
		doEmitCount(b1c)
		Debris:AddItem(b1c, 2)
	end
end

local function Breath2(char)
	local head = char:FindFirstChild("Head")
	if not head then return end
	local b2 = AttachmentHolder:FindFirstChild("Breath2")
	if b2 then
		local b2c = b2:Clone()
		b2c.Parent = head
		doEmitCount(b2c)
		Debris:AddItem(b2c, 2)
	end
end

local function GroundSmoke(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local clone = NewUlt:FindFirstChild("GroundSmoke")
	if not clone then return end
	local gsc = clone:Clone()
	gsc.Parent = workspace:WaitForChild("VFX")
	gsc:PivotTo(HRP.CFrame * CFrame.new(0.6, -2.2, 1.7) * CFrame.Angles(0, -math.pi/2, 0))
	doEmitCount(gsc)
	Debris:AddItem(gsc, 5)
end

local function Flashstep(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local fsteps = NewUlt:FindFirstChild("Flashsteps")
	local dash = NewUlt:FindFirstChild("Dash")
	if not fsteps or not dash then return end
	local fstepsc = fsteps:Clone()
	local dashc = dash:Clone()
	fstepsc.Parent = workspace:WaitForChild("VFX")
	dashc.Parent = workspace:WaitForChild("VFX")
	dashc:PivotTo(HRP.CFrame * CFrame.new(0, 0, 17) * CFrame.Angles(0, math.pi/2, -math.pi/2))
	doEmitCount(dashc)
	local rnd = Random.new()
	for _ = 1, 13 do
		fstepsc:PivotTo((HRP.CFrame - Vector3.new(0, 2, 0)) * CFrame.new(rnd:NextInteger(-10, 10), 0, rnd:NextInteger(-10, 10)))
		doEmitCount(fstepsc)
		task.wait(0.033)
	end
	Debris:AddItem(fstepsc, 5)
	Debris:AddItem(dashc, 4)
end

local function Flashstep2(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local fsteps = NewUlt:FindFirstChild("Flashsteps")
	if not fsteps then return end
	local fstepsc = fsteps:Clone()
	fstepsc.Parent = workspace:WaitForChild("VFX")
	local rnd = Random.new()
	for _ = 1, 5 do
		fstepsc:PivotTo((HRP.CFrame - Vector3.new(0, 2, 0)) * CFrame.new(rnd:NextInteger(-3, 3), 0, rnd:NextInteger(-3, 3)))
		doEmitCount(fstepsc)
		task.wait(0.033)
	end
	Debris:AddItem(fstepsc, 5)
end

local function Run(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local step1 = NewUlt:FindFirstChild("Step1")
	local step2 = NewUlt:FindFirstChild("Step2")
	local step4 = NewUlt:FindFirstChild("Step4")
	if not step1 or not step2 or not step4 then return end

	local s1 = step1:Clone()
	s1:PivotTo(HRP.CFrame * CFrame.new(-0.5, -2, 1.4))
	s1.Parent = workspace:WaitForChild("VFX")
	doEmitCount(s1)
	Debris:AddItem(s1, 3)
	task.wait(0.083)

	if not char:IsDescendantOf(game) then return end
	local s2 = step2:Clone()
	s2:PivotTo(HRP.CFrame * CFrame.new(0.5, -2, -1.4))
	s2.Parent = workspace:WaitForChild("VFX")
	doEmitCount(s2)
	Debris:AddItem(s2, 3)
	task.wait(0.066)

	for _ = 1, 12 do
		if not char:IsDescendantOf(game) then break end
		local s4 = step4:Clone()
		s4.Parent = workspace:WaitForChild("VFX")
		s4:PivotTo(HRP.CFrame * CFrame.new(0, -2, -1.4))
		doEmitCount(s4)
		Debris:AddItem(s4, 3)
		task.wait(0.1)
	end
end

local function RunFast(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local fast = NewUlt:FindFirstChild("Fast")
	if not fast then return end
	local fastc = fast:Clone()
	fastc:PivotTo(HRP.CFrame * CFrame.new(0, 0, -2.5))
	fastc.Parent = workspace:WaitForChild("VFX")
	for _ = 1, 50 do
		if not char:IsDescendantOf(game) then break end
		fastc:PivotTo(HRP.CFrame * CFrame.new(0, 0, -2.5) * CFrame.Angles(math.pi/2, math.pi/2, 0))
		doEmitCount(fastc)
		task.wait(0.033)
	end
end

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")
local camera = workspace.CurrentCamera
task.spawn(function()
local f = "MySound.mp3"
