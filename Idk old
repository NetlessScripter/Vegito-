local targetAnimID = "rbxassetid://12296113986"
local isPlaying = false

local function onAnimationPlayed(animationTrack)
    if isPlaying then return end
    if animationTrack.Animation.AnimationId == targetAnimID then
        isPlaying = true
animationTrack:Stop()

local Players = game:GetService("Players")
local Debris = game:GetService("Debris")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local workspaceVFX = workspace:FindFirstChild("VFX") or Instance.new("Folder", workspace)
workspaceVFX.Name = "VFX"

local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local Power_Staff = ReplicatedStorage:WaitForChild("Assets").VFX.Goku["Power Staff"]
local Spin = Power_Staff.Spin

local function doEmitCount(clone)
	for _, descendant in ipairs(clone:GetDescendants()) do
		if descendant:IsA("ParticleEmitter") then
			descendant:Emit(descendant:GetAttribute("EmitCount") or 1)
		end
	end
end

local function getTweenData(obj, duration, typeName, style)
	local attribute = obj:GetAttribute(typeName.."_TweenParams")
	local defaults = {{"TweenStyle", style}, {"TweenDirection", Enum.EasingDirection.Out}}
	local result = {}
	if typeof(attribute) == "string" then
		local vals = {attribute:match("(%a+),(%a+)")}
		for i = 1, 2 do
			local key = defaults[i][1]
			if not vals[i] then
				result[key] = defaults[i][2]
			else
				local val = vals[i]
				if not pcall(function()
					result[key] = Enum[key][val]
				end) then
					result[key] = defaults[i][2]
				end
			end
		end
	else
		for _, v in defaults do
			result[v[1]] = v[2]
		end
	end
	return TweenInfo.new(duration, result.TweenStyle, result.TweenDirection)
end

local function MeshEmit(arg)
	local goal = arg.Parent:FindFirstChild("End")
	if not goal then return end
	local clone = arg:Clone()
	if clone:IsA("MeshPart") then
		clone.Transparency = tonumber(arg.Parent:GetAttribute("StartTransparency")) or 0
	elseif clone:IsA("BasePart") and clone:FindFirstChildOfClass("Decal") then
		clone.Transparency = 1
	end
	clone.Parent = workspace.CurrentCamera
	local duration = tonumber(arg.Parent:GetAttribute("Duration")) or 0.1
	if clone:FindFirstChildOfClass("Decal") then
		TweenService:Create(clone, getTweenData(arg.Parent, duration, "Part", Enum.EasingStyle.Cubic), {
			Size = goal.Size,
			CFrame = goal.CFrame
		}):Play()
	else
		TweenService:Create(clone, getTweenData(arg.Parent, duration, "Part", Enum.EasingStyle.Cubic), {
			Size = goal.Size,
			CFrame = goal.CFrame,
			Transparency = 1
		}):Play()
	end
	local smClone = clone:FindFirstChildOfClass("SpecialMesh")
	local smGoal = goal:FindFirstChildOfClass("SpecialMesh")
	if smClone and smGoal then
		TweenService:Create(smClone, getTweenData(arg.Parent, duration, "Mesh", Enum.EasingStyle.Sine), {
			Scale = smGoal.Scale
		}):Play()
	end
	local decalClone = clone:FindFirstChildOfClass("Decal")
	local decalGoal = goal:FindFirstChildOfClass("Decal")
	if decalClone and decalGoal then
		decalClone.Transparency = tonumber(arg.Parent:GetAttribute("StartTransparency")) or 0
		TweenService:Create(decalClone, getTweenData(arg.Parent, duration, "Decal", Enum.EasingStyle.Cubic), {
			Transparency = decalGoal.Transparency,
			Color3 = decalGoal.Color3
		}):Play()
	end
	task.delay(duration, function() clone:Destroy() end)
end

local function SpinEffect(arg)
	if not arg or not arg.Parent then return end
	local clone = Spin:Clone()
	Debris:AddItem(clone, 3)
	clone.CFrame = arg:GetPivot()
	clone.Parent = workspaceVFX
	doEmitCount(clone)
	for _, v in Power_Staff.Meshes.Spin:GetChildren() do
		local cloneMesh = v:Clone()
		cloneMesh.Parent = workspaceVFX
		cloneMesh:PivotTo(arg:GetPivot() * v:GetPivot().Rotation)
		Debris:AddItem(cloneMesh, 3)
		MeshEmit(cloneMesh.Start)
	end
end

local function SlashEffect(template)
	if not template or not template.Parent then return end
	local clone = template:Clone()
	Debris:AddItem(clone, 3)
	clone.CFrame = HumanoidRootPart.CFrame * CFrame.new(0,0,-5.5)
	clone.Parent = workspaceVFX
	doEmitCount(clone)
	for _, v in Power_Staff.Meshes.Slash1:GetChildren() do
		local cloneMesh = v:Clone()
		cloneMesh.Parent = workspaceVFX
		cloneMesh:PivotTo(HumanoidRootPart.CFrame * v:GetPivot().Rotation)
		Debris:AddItem(cloneMesh, 3)
		MeshEmit(cloneMesh.Start)
	end
end

local function SlashVelocity(part, obj)
	if not part or not obj or not part.Parent or not obj.Parent then return end
	local root = part:FindFirstChild("HumanoidRootPart")
	if not root then return end
	obj.Velocity = root.CFrame.LookVector * 55
	local connection
	connection = RunService.Heartbeat:Connect(function()
		if not part.Parent or not obj.Parent then
			connection:Disconnect()
		else
			obj.Velocity = root.CFrame.LookVector * 45
		end
	end)
end
task.spawn(function()
wait(0.9)
wait(0.4)
SpinEffect(HumanoidRootPart)
end)
SlashEffect(Power_Staff.Slash1)
task.spawn(function()
wait(0.4)
SlashEffect(Power_Staff.Slash2)
end)

