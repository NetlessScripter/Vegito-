local AnimationPlayed = false

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local function onAnimationPlayed(animationTrack)
    if AnimationPlayed then return end

    if animationTrack.Animation.AnimationId == "rbxassetid://" .. AnimTarget then
        AnimationPlayed = true
animationTrack:Stop()
local cam = workspace.CurrentCamera
local TweenService = game:GetService("TweenService")

local info1 = TweenInfo.new(0.7, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local info2 = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local info3 = TweenInfo.new(1.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

local tween1 = TweenService:Create(cam, info1, {FieldOfView = 60})
local tween2 = TweenService:Create(cam, info2, {FieldOfView = 80})
local tween3 = TweenService:Create(cam, info3, {FieldOfView = 70})

tween1:Play()
tween1.Completed:Connect(function()
    tween2:Play()
    tween2.Completed:Connect(function()
        tween3:Play()
    end)
end)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local KiKicks = ReplicatedStorage.Assets.VFX.Goku.KiKicks
local LocalPlayer = game.Players.LocalPlayer
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local function Velocity1()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    if not character or not character.Parent then return end
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local NumberValue = Instance.new("NumberValue")
    NumberValue.Value = 50
    TweenService:Create(NumberValue, TweenInfo.new(0.15, Enum.EasingStyle.Quad), {Value = 7.5}):Play()

    local connection
    connection = RunService.RenderStepped:Connect(function()
        if not hrp.Parent then
            connection:Disconnect()
            return
        end
        hrp.Velocity = hrp.CFrame.LookVector * NumberValue.Value
        if NumberValue.Value <= 7.6 then
            connection:Disconnect()
            NumberValue:Destroy()
        end
    end)
end

local function Hit(target, hitIndex)
    local clone = KiKicks:Clone()
    Debris:AddItem(clone, 10)
    local hrp = target:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    clone.CFrame = hrp.CFrame * CFrame.Angles(0, math.pi, 0)
    clone.Parent = workspace.VFX
    if hitIndex <= 2 then
        for _, emitter in ipairs(clone.hit1:GetDescendants()) do
            if emitter:IsA("ParticleEmitter") then
                emitter:Emit(emitter:GetAttribute("EmitCount") or 1)
            end
        end
    else
        for _, emitter in ipairs(clone.hit2:GetDescendants()) do
            if emitter:IsA("ParticleEmitter") then
                emitter:Emit(emitter:GetAttribute("EmitCount") or 1)
            end
        end
    end
end

local function Final(target)
    local clone = KiKicks:Clone()
    Debris:AddItem(clone, 10)
    local hrp = target:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    clone.CFrame = hrp.CFrame * CFrame.Angles(0, math.pi, 0)
    clone.Parent = workspace.VFX
    for _, emitter in ipairs(clone.hit3:GetDescendants()) do
        if emitter:IsA("ParticleEmitter") then
            emitter:Emit(emitter:GetAttribute("EmitCount") or 1)
        end
    end
end

