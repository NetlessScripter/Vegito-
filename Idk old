local AnimTarget = "13376962659"
local AnimationPlayed = false

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local RS = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Lighting = game:GetService("Lighting")
local Atmosphere = Lighting:FindFirstChildOfClass("Atmosphere")
local Clouds = workspace.Terrain:FindFirstChildOfClass("Clouds")
local Assets = RS:WaitForChild("Assets")
local Debris = game:GetService("Debris")
local lp = Players.LocalPlayer

local function Recolor(p)
	for _, v in pairs(p:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v.Color = ColorSequence.new(Color3.fromRGB(255, 255, 0))
		end
	end
end

local function VisualsStart(arg1)
	if arg1.Parent then
		local conn
		local c = Assets.VFX.ultthukuna:Clone()
		Recolor(c)
		task.delay(0.4, function()
			if arg1.Parent then
				for _, v in pairs(arg1:GetDescendants()) do
					if v.Name ~= "HumanoidRootPart" and (v:IsA("Part") or v:IsA("BasePart") or v:IsA("Decal")) then
						v:SetAttribute("OldTransparency", v.Transparency)
						TweenService:Create(v, TweenInfo.new(0.15, Enum.EasingStyle.Linear), {Transparency = 1}):Play()
					end
				end
				c.Parent = workspace:WaitForChild("VFX")
				c.CFrame = arg1:WaitForChild("HumanoidRootPart").CFrame
				for _, v in pairs(c:GetDescendants()) do
					if v:IsA("ParticleEmitter") and not string.find(v.Name, "EMIT") then
						v.Enabled = true
					end
				end
				conn = RunService.RenderStepped:Connect(function()
					if arg1.Parent and arg1:FindFirstChild("HumanoidRootPart") and c.Parent then
						c.CFrame = arg1.HumanoidRootPart.CFrame
					else
						if conn then conn:Disconnect() end
					end
				end)
			end
		end)
		task.delay(2, function()
			if arg1.Parent then
				for _, v in pairs(arg1:GetDescendants()) do
					if v.Name ~= "HumanoidRootPart" and (v:IsA("Part") or v:IsA("BasePart") or v:IsA("Decal")) then
						TweenService:Create(v, TweenInfo.new(0.15, Enum.EasingStyle.Linear), {
							Transparency = v:GetAttribute("OldTransparency") or 0
						}):Play()
					end
				end
				if conn and conn.Connected then conn:Disconnect() end
				Debris:AddItem(c, 2)
				for _, v in pairs(c:GetDescendants()) do
					if v:IsA("ParticleEmitter") then
						if string.find(v.Name, "EMIT") then
							v:Emit(v:GetAttribute("EmitCount"))
						end
						v.Enabled = false
					end
				end
			end
		end)
	end
end

local function onAnimationPlayed(animationTrack)
    if AnimationPlayed then return end
    if animationTrack.Animation.AnimationId == "rbxassetid://" .. AnimTarget then
        AnimationPlayed = true
        animationTrack:Stop()
        local char = lp.Character or lp.CharacterAdded:Wait()
        VisualsStart(char)

local p = game.Players.LocalPlayer
local c = p.Character or p.CharacterAdded:Wait()
local h = c:WaitForChild("Humanoid")

local a = Instance.new("Animation")
a.AnimationId = "rbxassetid://13499771836"
local track = h:LoadAnimation(a)

track:Play(0) 
track.TimePosition = 1.5
track:AdjustSpeed(1.5)

        task.wait(1.6)
        AnimationPlayed = false
    end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)
local v17001 = game.ReplicatedStorage.Resources.FSSlash
v17001.Color = Color3.new(1, 1, 0)
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local Players = game:GetService("Players")
local FALLINGSERPENT = ReplicatedStorage.Assets.VFX.Minos.FALLINGSERPENT
local workspaceVFX = workspace:FindFirstChild("VFX") or Instance.new("Folder", workspace)
workspaceVFX.Name = "VFX"

local function doEmitCount(obj)
	for _, v in pairs(obj:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 10)
		end
	end
end

local function Hit(target)
	local clone = FALLINGSERPENT.Hit:Clone()
	clone.Parent = workspaceVFX
	Debris:AddItem(clone, 5)
	if clone:IsA("BasePart") or clone:IsA("Model") then
		clone.CFrame = target.CFrame + Vector3.new(0, 1.21, 0)
	end
	doEmitCount(clone)
end

local function Jump(target)
	local clone = FALLINGSERPENT.Jump:Clone()
	clone.Parent = workspaceVFX
	Debris:AddItem(clone, 5)
	if clone:IsA("BasePart") or clone:IsA("Model") then
		clone.CFrame = target.CFrame + Vector3.new(0, 0.662, 0)
	end
	doEmitCount(clone)
end

local function Kick(target, serpentModel)
	local clone = FALLINGSERPENT.Kick:Clone()
	clone.Parent = workspaceVFX
	Debris:AddItem(clone, 10)
	if clone:IsA("BasePart") then
		clone.CFrame = (target.CFrame + Vector3.new(0, 72, 0)) * CFrame.Angles(0.7853981633974483, 0, 0) * CFrame.new(0, 0, -3)
	end
	doEmitCount(clone)

	task.delay(0.1, function()
		if serpentModel and serpentModel.Parent then
			local var18 = serpentModel
			for _, v in pairs(FALLINGSERPENT.Snake.SnakeAttachments:GetChildren()) do
				if v:IsA("Attachment") then
					v:Clone().Parent = var18.Snake["Bone.006"]
				end
			end
			for _, v in pairs(FALLINGSERPENT.Snake.Trails:GetChildren()) do
				if v:IsA("Trail") then
					local cloneV = v:Clone()
					cloneV.Parent = var18.Snake
					cloneV.Attachment0 = var18.Snake["Bone.006"]:FindFirstChild("Attachment0")
					cloneV.Attachment1 = var18.Snake["Bone.006"]:FindFirstChild("Attachment1")
					cloneV.Enabled = true
				end
			end
			for _, v in pairs(var18:GetDescendants()) do
				if v:IsA("Beam") or v:IsA("ParticleEmitter") then
					v.Enabled = true
				end
			end
		end
	end)
end

local function Impact(target)
	local clone = FALLINGSERPENT.Ground:Clone()
	clone.Parent = workspaceVFX
	Debris:AddItem(clone, 10)
	if clone:IsA("BasePart") then
		clone.Position = target.Position - Vector3.new(0, 0.1, 0)
	end
	doEmitCount(clone)
end

local AnimTarget = "13294790250"
local AnimationPlayed = false

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local humanoidRoot = character:WaitForChild("HumanoidRootPart")
local serpent = FALLINGSERPENT.Snake:Clone()
serpent.Parent = workspaceVFX

local function onAnimationPlayed(animationTrack)
	if AnimationPlayed then return end

	if animationTrack.Animation.AnimationId == "rbxassetid://" .. AnimTarget then
		AnimationPlayed = true
		animationTrack:Stop()

		local anim = Instance.new("Animation")
		anim.AnimationId = "rbxassetid://16719107050"

		local animTrack = humanoid:LoadAnimation(anim)
		animTrack:Play()
		animTrack.TimePosition = 1

		task.wait(0.4)
		Kick(humanoidRoot, serpent)
		task.wait(0.6)
		Impact(humanoidRoot)

		task.wait(1.6)
		AnimationPlayed = false
	end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)

    end
end

local function setup()
    checkAttribute()
    player:GetAttributeChangedSignal("Character"):Connect(checkAttribute)
end

player.CharacterAdded:Connect(setup)
setup()

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local blockedIDs = {
    ["rbxassetid://10456925537"] = true,
    ["rbxassetid://12771909201"] = true,
    ["rbxassetid://10457021115"] = true,
    ["rbxassetid://7556019578"] = true,
    ["rbxassetid://3183222154"] = true,
}

task.spawn(function()
    while true do
        for _, obj in ipairs(workspace:GetDescendants()) do
            if obj:IsA("Sound") and blockedIDs[obj.SoundId] then
                obj:Destroy()
            end
        end
        task.wait(0.01)
    end
end)

end

for _, player in ipairs(Players:GetPlayers()) do
    if IsAllowed(player) and player.Character then
        ShowEffect(player)
    end
end
