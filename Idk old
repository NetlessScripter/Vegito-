local Debris = game:GetService("Debris")
local LocalPlayer = game.Players.LocalPlayer

local Assets = ReplicatedStorage:WaitForChild("Assets")

local function doEmitCount(particleHolder)
	for _, emitter in ipairs(particleHolder:GetChildren()) do
		if emitter:IsA("ParticleEmitter") then
			emitter:Emit(15)
		end
	end
end

local function toggleAllEmitters(obj, state)
	for _, v in ipairs(obj:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v.Enabled = state
		end
	end
end

local function weld(part0, part1)
	local w = Instance.new("WeldConstraint")
	w.Part0 = part0
	w.Part1 = part1
	w.Parent = part1
	return w
end

local function Speed(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		local fx = Assets.VFX.Minos.FREEDOM.Lines.One
		local clone1 = fx.Holder.fxLines:Clone()
		local clone2 = fx.fxLines:Clone()
		clone2:PivotTo(hrp.CFrame)
		clone1.Parent = character.Torso
		clone2.Parent = character
		local w = weld(clone2, hrp)
		toggleAllEmitters(clone1, true)
		toggleAllEmitters(clone2, true)
		task.delay(1, function()
			if character:IsDescendantOf(game) then
				toggleAllEmitters(clone1, false)
				toggleAllEmitters(clone2, false)
				Debris:AddItem(clone1, 3)
				Debris:AddItem(clone2, 3)
				Debris:AddItem(w, 3)
			end
		end)
	end
end

local function GroundSmoke(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		local fx = Assets.VFX.Minos.FREEDOM.Holder.fxInBetween:Clone()
		fx.Parent = hrp
		doEmitCount(fx)
		Debris:AddItem(fx, 3)
	end
end

local function Rise(character)
    local humanoid = character:WaitForChild("Humanoid")
    for _, track in ipairs(humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
    end
    wait(0.01)

    local animation = Instance.new("Animation")
    animation.AnimationId = "rbxassetid://15503060232"
    local track = humanoid:LoadAnimation(animation)
    track:Play()

    local hrp = character:FindFirstChild("HumanoidRootPart")
    if hrp then
        local bv = Instance.new("BodyVelocity")
        bv.Velocity = Vector3.new(0, 100, 0)
        bv.MaxForce = Vector3.new(0, math.huge, 0)
        bv.Parent = hrp
        task.delay(1, function()
            bv:Destroy()

    track:Stop()

local plr = game.Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")

local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://12684390285"

local track = hum:LoadAnimation(anim)
track.TimePosition = 0.3
track:Play()

local happened = false

local function onTouchedGround()
    if happened then return end
    happened = true

    track:Stop()
    GroundSmoke(LocalPlayer.Character)

    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Replication = ReplicatedStorage.Replication
    local plr = game.Players.LocalPlayer
    local char = plr.Character or plr.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    firesignal(Replication.OnClientEvent, {
        Intensity = 4,
        Effect = "Camshake"
    })

    firesignal(Replication.OnClientEvent, {
        Effect = "Ground Crater",
        start = hrp.Position,
        Seed = 115442933,
        ["end"] = Vector3.new(0, -7, 0),
        nosound = true
    })

    firesignal(Replication.OnClientEvent, {
        size = 4,
        nosmoke = false,
        Seed = 1433240775,
        amount = 6,
        start = hrp.Position,
        Effect = "Ground Crater",
        ["end"] = Vector3.new(0, -14, 0),
        nosound = false
    })

    firesignal(Replication.OnClientEvent, {
        size = 2,
        nosmoke = true,
        Seed = 1433240775,
        amount = 6,
        start = hrp.Position,
        Effect = "Ground Crater",
        ["end"] = Vector3.new(0, -14, 0),
        nosound = true
    })
end

hum.StateChanged:Connect(function(_, newState)
    if newState == Enum.HumanoidStateType.Landed then
        onTouchedGround()
    end
end)

        end)

        local fx = Assets.VFX.Minos.FREEDOM.Lines.Two
        local clone1 = fx.Holder.fxLines:Clone()
        local clone2 = fx.fxLines:Clone()
        local clone3 = fx.Ground:Clone()
        clone2:PivotTo(hrp.CFrame * CFrame.Angles(0, 3, 0) * CFrame.Angles(0, 0, math.pi/2))
        clone3:PivotTo(hrp.CFrame * CFrame.new(0, -3, 0) * CFrame.Angles(0, 0, math.pi/2))
        clone1.Parent = character.Torso
        clone2.Parent = character
        clone3.Parent = character
        toggleAllEmitters(clone1, true)
        toggleAllEmitters(clone2, true)
        toggleAllEmitters(clone3, true)
        local w1 = weld(clone2, hrp)
        local w2 = weld(clone3, hrp)
        task.delay(1, function()
            if character:IsDescendantOf(game) then
                toggleAllEmitters(clone1, false)
                toggleAllEmitters(clone2, false)
                toggleAllEmitters(clone3, false)
                Debris:AddItem(clone1, 3)
                Debris:AddItem(clone2, 3)
                Debris:AddItem(clone3, 3)
                Debris:AddItem(w1, 3)
                Debris:AddItem(w2, 3)
            end
        end)
    end
end

local function Aura(character)
	local torso = character:FindFirstChild("Torso")
	if torso then
		local center = Assets.VFX.Minos.FREEDOM.Holder.fxCenter:Clone()
		center.Parent = torso
		doEmitCount(center)
		Debris:AddItem(center, 3)
	end
	for _, part in ipairs(character:GetChildren()) do
		if part:IsA("Part") and table.find({"Head","Left Arm","Right Arm","Torso","Left Leg","Right Leg"}, part.Name) then
			for _, emitter in ipairs(Assets.VFX.Minos.FREEDOM.AuraEmitters:GetChildren()) do
				local cloneEmitter = emitter:Clone()
				if cloneEmitter:IsA("ParticleEmitter") then
					cloneEmitter.Parent = part
					cloneEmitter.Enabled = true
					task.delay(45, function()
						cloneEmitter.Enabled = false
						Debris:AddItem(cloneEmitter, 2)
					end)
				end
			end
		end
	end
end

wait(0.2)
Speed(LocalPlayer.Character)
wait(1.9)
Aura(LocalPlayer.Character)
task.spawn(function()
wait(0.6)
GroundSmoke(LocalPlayer.Character)
wait(1)
GroundSmoke(LocalPlayer.Character)
end)
wait(2.3)
Rise(LocalPlayer.Character)

        task.wait(1.6)
        AnimationPlayed = false
    end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)

local Debris = game:GetService("Debris")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local JUDGEMENT = ReplicatedStorage:WaitForChild("Assets").VFX.Minos.JUDGEMENT
local LocalPlayer = game:GetService("Players").LocalPlayer
local TweenService = game:GetService("TweenService")

local function meshHandler(cf, meshes)
	for _, v in meshes do
		local clone = v.mesh:Clone()
		clone.Parent = workspace.VFX
		clone:PivotTo(cf * v.offset)
		Debris:AddItem(clone, 1)
		if clone:FindFirstChild("Start") then
			for _, e in clone.Start:GetDescendants() do
				if e:IsA("ParticleEmitter") then
					e:Emit(10)
				end
			end
		end
	end
end

local function ArmFlashStart(char)
	local left = char:FindFirstChild("Left Arm")
	local right = char:FindFirstChild("Right Arm")
	if left and right then
		local fx1 = JUDGEMENT.AttachmentStorage.fxFlashStart:Clone()
		local fx2 = JUDGEMENT.AttachmentStorage.fxFlashStart:Clone()
		fx1.Parent = left
		fx2.Parent = right
		for _, e in fx1:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = true e:Emit(10) end end
		for _, e in fx2:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = true e:Emit(10) end end
		Debris:AddItem(fx1, 3)
		Debris:AddItem(fx2, 3)
		task.delay(1, function()
			if char:IsDescendantOf(game) then
				for _, e in fx1:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = false end end
				for _, e in fx2:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = false end end
			end
		end)
	end
end

local function FirstHit(char)
	local hrp = char:FindFirstChild("HumanoidRootPart")
	if hrp then
		local fx = JUDGEMENT.AttachmentStorage.fxFirstHit:Clone()
		fx.Parent = hrp
		for _, e in fx:GetDescendants() do if e:IsA("ParticleEmitter") then e:Emit(10) end end
		Debris:AddItem(fx, 6)
		meshHandler(hrp.CFrame, {
			{mesh = JUDGEMENT.Meshes.FirstHit.Mesh1, offset = CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.FirstHit.Mesh2, offset = CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.FirstHit.Mesh3, offset = CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.FirstHit.Mesh4, offset = CFrame.new(0, 0, 12) * CFrame.Angles(0, -math.pi/2, 0)}
		})
	end
end

local function Slam(char)
	local hrp = char:FindFirstChild("HumanoidRootPart")
	if hrp then
		local fx = JUDGEMENT.AttachmentStorage.fxSlam:Clone()
		fx.Parent = hrp
		for _, e in fx:GetDescendants() do if e:IsA("ParticleEmitter") then e:Emit(10) end end
		Debris:AddItem(fx, 6)
		meshHandler(hrp.CFrame, {
			{mesh = JUDGEMENT.Meshes.Slam.Mesh1, offset = CFrame.new(0, -2, -4)},
			{mesh = JUDGEMENT.Meshes.Slam.Mesh2, offset = CFrame.new(0, 3, -3)},
			{mesh = JUDGEMENT.Meshes.Slam.Mesh3, offset = CFrame.new(0, 15, -3)},
			{mesh = JUDGEMENT.Meshes.Slam.Mesh4, offset = CFrame.new(0, 2, -3) * CFrame.Angles(0, 0, math.pi/2)},
			{mesh = JUDGEMENT.Meshes.Slam.Mesh5, offset = CFrame.new(0, 5, -3)},
			{mesh = JUDGEMENT.Meshes.Slam.Mesh6, offset = CFrame.new(0, -2, -6)}
		})
	end
end

local function LastHit(char)
	local hrp = char:FindFirstChild("HumanoidRootPart")
	if hrp then
		local fx = JUDGEMENT.AttachmentStorage.fxLastHit:Clone()
		fx.Parent = hrp
		for _, e in fx:GetDescendants() do if e:IsA("ParticleEmitter") then e:Emit(10) end end
		Debris:AddItem(fx, 6)
		meshHandler(hrp.CFrame, {
			{mesh = JUDGEMENT.Meshes.LastHit.Mesh1, offset = CFrame.new(0, 0, -16) * CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.LastHit.Mesh2, offset = CFrame.new(0, 0, -8) * CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.LastHit.Mesh3, offset = CFrame.new(0, 0, -6) * CFrame.Angles(math.pi/2, 0, 0)},
			{mesh = JUDGEMENT.Meshes.LastHit.Mesh4, offset = CFrame.new(0, 0, -5) * CFrame.Angles(0, math.pi/2, 0)}
		})
		local beams = JUDGEMENT.Beams:Clone()
		beams.Parent = workspace.VFX
		Debris:AddItem(beams, 20)
		beams:PivotTo(hrp.CFrame * CFrame.Angles(0, -math.pi/2, 0))
		for _, v in beams:GetDescendants() do
			if v:IsA("Beam") then
				v.Enabled = true
				TweenService:Create(v, TweenInfo.new(3, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
					LightEmission = 1,
					TextureSpeed = 0.5,
					Brightness = 0
				}):Play()
			end
		end
	end
end

local function Flicker(char, delayTime)
	local torso = char:FindFirstChild("Torso")
	if torso then
		local fx = JUDGEMENT.AttachmentStorage.fxFlash:Clone()
		fx.Parent = torso
		for _, e in fx:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = true e:Emit(10) end end
		Debris:AddItem(fx, 6)
		task.delay(delayTime, function()
			if fx:IsDescendantOf(game) then
				for _, e in fx:GetDescendants() do if e:IsA("ParticleEmitter") then e.Enabled = false end end
			end
		end)
	end
end

