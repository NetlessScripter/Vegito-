local function IntroWind()
	local wind = Assets.VFX.Minos.Serpent.Effects.Handler.WindUp:Clone()
	wind.Parent = hrp
	for _, v in pairs(wind:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 10)
		end
	end
	task.delay(4, function() wind:Destroy() end)
end

local function ThrowVFX()
	local hitEffect = Assets.VFX.Minos.Serpent.Meshes.Hit:Clone()
	hitEffect.Parent = workspace.VFX
	for _, part in pairs(hitEffect:GetChildren()) do
		if part:GetAttribute("Offset") then
			part:PivotTo(hrp.CFrame * CFrame.new(part:GetAttribute("Offset")))
		else
			part:PivotTo(hrp.CFrame)
		end
		for _, v in pairs(part:GetDescendants()) do
			if v:IsA("ParticleEmitter") then
				v:Emit(v:GetAttribute("EmitCount") or 10)
			end
		end
	end
	hitEffect:Destroy()
end

local function BiteVFX()
	local Handler = Assets.VFX.Minos.Serpent.Effects.Handler
	local bite = Handler.Bite:Clone()
	local whoosh = Handler.Whoosh:Clone()
	bite.Parent = hrp
	whoosh.Parent = hrp
	for _, v in pairs(bite:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 10)
		end
	end
	for _, v in pairs(whoosh:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 10)
		end
	end
	Debris:AddItem(bite, 2)
	Debris:AddItem(whoosh, 2)
end
wait(0.4)
IntroWind()
wait(0.2)
ThrowVFX()
BiteVFX()
wait(0.2)
ThrowVFX()
BiteVFX()
wait(0.2)
ThrowVFX()
BiteVFX()
wait(0.1)
ThrowVFX()
BiteVFX()
wait(0.1)
ThrowVFX()
BiteVFX()
wait(0.2)
ThrowVFX()
BiteVFX()
wait(0.2)
ThrowVFX()
BiteVFX()
wait(0.4)
ThrowVFX()
BiteVFX()

        task.wait(1.6)
        AnimationPlayed = false
    end
end

