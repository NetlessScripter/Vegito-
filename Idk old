local ReplicatedStorage = game:GetService("ReplicatedStorage")
local DivergantFinisher = ReplicatedStorage:WaitForChild("Assets").VFX.Yuji:WaitForChild("DivergantFinisher")

local function attachEffect(parent, effect, duration)
	local clone = effect:Clone()
	clone.Parent = parent
	for _, v in ipairs(clone:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(1)
		end
	end
	task.delay(duration, function()
		clone:Destroy()
	end)
end

local function Snow(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxBfEm, 3)
		attachEffect(hrp, DivergantFinisher.fxAttach.fxBfEn, 1)
	end
end

local function TargetGround1(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxTargetGr1, 6)
	end
end

local function TargetGround2(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxTargetGr2, 6)
	end
end

local function TargetGround3(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxTargetGr3, 3)
	end
end

local function AuraF(character)
	local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
	if torso then
		attachEffect(torso, DivergantFinisher.fxAttach.fxAuraFarming, 5)
	end
end

local function Afterimage1(character)
	local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
	if torso then
		attachEffect(torso, DivergantFinisher.fxAttach.fxAfterimage, 1)
	end
end

local function AfterImage2(character)
	local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
	if torso then
		attachEffect(torso, DivergantFinisher.fxAttach.fxAfterimage, 5)
	end
end

local function Appear(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
	local rightLeg = character:FindFirstChild("Right Leg") or character:FindFirstChild("RightLowerLeg")

	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxAppear, 3)
	end
	if torso then
		attachEffect(torso, DivergantFinisher.fxAttach.fxAfterimage, 1)
	end
	if rightLeg then
		attachEffect(rightLeg, DivergantFinisher.fxAttach.fxCe, 2)
	end
end

local function Step1(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxStep1, 3) end
end
local function Step2(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxStep2, 3) end
end
local function Step3(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxStep3, 3) end
end
local function Step4(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxStep4, 3) end
end
local function Step5(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxStep5, 3) end
end

local function PreSlam(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then
		attachEffect(hrp, DivergantFinisher.fxAttach.fxPreSlam, 3)
		attachEffect(hrp, DivergantFinisher.fxAttach.fxPreSlamEn, 1)
	end
end

local function Slam(character)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if hrp then attachEffect(hrp, DivergantFinisher.fxAttach.fxSlam, 3) end
end

