local player = Players.LocalPlayer

local function checkAttribute()
    if player:GetAttribute("Character") == "Ninja" then

local player = game:GetService("Players").LocalPlayer
local bar = player.PlayerGui.Bar.MagicHealth.Health.Bar
local targetColor = Color3.fromRGB(255, 215, 0)

for _, obj in ipairs(bar:GetDescendants()) do
    if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
        obj.ImageColor3 = targetColor
    elseif obj:IsA("Frame") then
        obj.BackgroundColor3 = targetColor
    end
end

