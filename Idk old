			Scale = End_2.Mesh.Scale;
		}):Play()
		task.wait(0.2)
		End_2:Destroy()
		Start:Destroy()
	elseif arg2 == 4 then
		local clone_18 = Assets_upvr.VFX.LimitlessCombo.Presets.Preset3:Clone()
