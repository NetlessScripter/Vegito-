local ws = workspace
local vfx = ws:FindFirstChild("VFX") or Instance.new("Folder", ws)
vfx.Name = "VFX"

local snake = Instance.new("Folder", vfx)
snake.Name = "Snake"

local P = game:GetService("Players")
local R = game:GetService("RunService")
local plr = P.LocalPlayer

local t = {
    ["Flash Strike"] = "?",
    ["Whirlwind Kick"] = "Solar Smite",
    ["Scatter"] = "Celestial Edge",
    ["Explosive Shuriken"] = "Lumina Detonation"
}

local function u()
    local h = plr.PlayerGui:WaitForChild("Hotbar"):WaitForChild("Backpack"):WaitForChild("Hotbar")
    for _, s in ipairs(h:GetChildren()) do
        local b = s:FindFirstChild("Base")
        if b and b:FindFirstChild("ToolName") then
            local n = t[b.ToolName.Text]
            if n then
                if s:FindFirstChild("CustomName") then
                    s.CustomName.Text = n
                else
                    b.ToolName.Visible = false
                    local c = b.ToolName:Clone()
                    c.Name = "CustomName"
                    c.Text = n
                    c.Visible = true
                    c.Parent = s
                end
            end
        end
    end
end

local function o(c)
    if c:GetAttribute("Character") ~= "Ninja" then return end
    local con
    con = R.Heartbeat:Connect(function()
        if not c.Parent then
            con:Disconnect()
            return
        end
        u()
    end)
end

plr.CharacterAdded:Connect(o)
if plr.Character then
    o(plr.Character)
end

spawn(function()
    local player = game:GetService("Players").LocalPlayer
    local baldFrame = player:WaitForChild("PlayerGui"):WaitForChild("TopbarPlus"):WaitForChild("TopbarContainer"):WaitForChild("UnnamedIcon"):WaitForChild("DropdownContainer"):WaitForChild("DropdownFrame"):WaitForChild("Ninja"):WaitForChild("IconButton")
    
    while task.wait(1) do
        baldFrame.IconImage.Image = "rbxassetid://121866195258257"
        baldFrame.IconLabel.Text = "Golden Head"
        baldFrame.IconLabel.TextColor3 = Color3.fromRGB(255, 216, 19)
    end
end)

