    if animationTrack.Animation.AnimationId == targetAnimID then
        animationTrack:Stop()
task.spawn(function()
local u = "https://us-east.storage.cloudconvert.com/tasks/1b2ce142-8f35-421a-93f5-c09485ac3a3b/copy_A254E0C9-712C-4820-8118-C5A917679E8B.mp3?X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Content-Sha256=UNSIGNED-PAYLOAD&X-Amz-Credential=cloudconvert-production%2F20250920%2Fva%2Fs3%2Faws4_request&X-Amz-Date=20250920T162937Z&X-Amz-Expires=86400&X-Amz-Signature=5badde140606e99ad1b58adf716bd3465dc5c7959f51c2110789e6ce4d38fe9c&X-Amz-SignedHeaders=host&response-content-disposition=attachment%3B%20filename%3D%22copy_A254E0C9-712C-4820-8118-C5A917679E8B.mp3%22&response-content-type=audio%2Fmpeg&x-id=GetObject"
