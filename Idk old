local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Replication = ReplicatedStorage:WaitForChild("Replication")

local mt = getrawmetatable(game)
setreadonly(mt, false)

local oldNamecall = mt.__namecall
mt.__namecall = newcclosure(function(self, ...)
    local args = {...}
    if self == Replication and getnamecallmethod() == "FireClient" then
        if type(args[2]) == "table" and args[2].Effect == "Serious Punch Camera" then
            args[2] = {}
        end
    end
    return oldNamecall(self, unpack(args))
end)

setreadonly(mt, true)

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local toolNameMap = {
    ["Normal Punch"] = "Reversal Red",
    ["Consecutive Punches"] = "Limitless Beatdown",
    ["Shove"] = "Limitless Kick",
    ["Uppercut"] = "?",
    ["Serious Punch"] = "Lapse Blue Max",
    ["Omni Directional Punch"] = "?",
    ["Death Counter"] = "Reversal Red Max",
    ["Table Flip"] = "Hollow Purple"
}

local function updateHotbar(char)
    local hotbar = player.PlayerGui:WaitForChild("Hotbar"):WaitForChild("Backpack"):WaitForChild("Hotbar")
    for _, slot in ipairs(hotbar:GetChildren()) do
        local base = slot:FindFirstChild("Base")
        if base and base:FindFirstChild("ToolName") then
            local txt = base.ToolName.Text
            local newName = toolNameMap[txt]
            if newName then
                if not slot:FindFirstChild("CustomName") then
                    base.ToolName.Visible = false
                    local clone = base.ToolName:Clone()
                    clone.Name = "CustomName"
                    clone.Text = newName
                    clone.Visible = true
                    clone.Parent = slot
                else
                    base.ToolName.Visible = false
                    slot.CustomName.Text = newName
                end
            end
        end
    end
end

local function checkAndUpdate(char)
    spawn(function()  -- Run in a new thread so it doesn't block
        while char.Parent do
            if char:GetAttribute("Character") == "Bald" then
                updateHotbar(char)
            end
            wait(0.5)
        end
    end)
end

player.CharacterAdded:Connect(checkAndUpdate)
if player.Character then
    checkAndUpdate(player.Character)
end

