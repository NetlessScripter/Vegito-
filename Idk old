end

local rs = game:GetService("ReplicatedStorage")
local db = game:GetService("Debris")
local ts = game:GetService("TweenService")
local lp = game.Players.LocalPlayer

local function grab_hit(a, b)
    if not a or not b then return end
    if not a.Parent or not b.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local c = rs.Assets.VFX["Dragon Throw Use"].grab:Clone()
    c.Parent = hrp
    db:AddItem(c, 1)
end

local function wind(a, b)
    if not a or not b then return end
    if not a.Parent or not b.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local c = rs.Assets.VFX["Dragon Throw Use"].spin:Clone()
    c.Parent = hrp
    for _, v in pairs(c:GetDescendants()) do
        if v:IsA("ParticleEmitter") then
            v.Enabled = true
        end
    end
    db:AddItem(c, 3)
    task.wait(1)
    for _, v in pairs(c:GetDescendants()) do
        if v:IsA("ParticleEmitter") then
            v.Enabled = false
        end
    end
end

local function grab_launch(a, b)
    if not a or not b then return end
    if not a.Parent or not b.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local c = rs.Assets.VFX["Dragon Throw Use"].throw:Clone()
    c.Parent = hrp
    db:AddItem(c, 2)
end

local function Finisher_PunchVFX(a)
    if not a then return end
    if not a.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local c = rs.Assets.VFX.DragonThrow_Finisher:Clone()
    c.Parent = workspace.VFX
    c.CFrame = hrp.CFrame * CFrame.Angles(0, math.pi, 0)
    db:AddItem(c, 6)
end

local function Finisher_VictimPush(a, b)
    if not a or not b then return end
    if not a.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local hum = a:FindFirstChild("Humanoid")
    if not hum then return end
    local t = ts:Create(hrp, TweenInfo.new(0.5, Enum.EasingStyle.Sine), {CFrame = b})
    t.Completed:Connect(function()
        local t2 = ts:Create(hrp, TweenInfo.new(2.25, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            CFrame = b * CFrame.new(0,0,5) + Vector3.new(0,15,0)
        })
        hum:GetPropertyChangedSignal("Health"):Connect(function()
            if hum.Health <= 0 and t2.PlaybackState == Enum.PlaybackState.Playing then
                t2:Cancel()
            end
        end)
        t2:Play()
    end)
    t:Play()
end

local function Finisher_JumpTween(a, b)
    if not a or not b then return end
    if not a.Parent then return end
    local hrp = a:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local rp = RaycastParams.new()
    rp.FilterType = Enum.RaycastFilterType.Include
    rp.FilterDescendantsInstances = {workspace.Map.Parts}
    local r = workspace:Raycast(hrp.Position + Vector3.new(0,1,0), Vector3.new(0,-8,0), rp)
    if r then
        task.spawn(function()
            for _ = 1, 10 do
                local p = Instance.new("Part")
                p.Size = Vector3.new(1,1,1)
                p.Position = r.Position + Vector3.new(0,1,0)
                p.Anchored = false
                p.Parent = workspace
                p.Velocity = Vector3.new(0,-7,0)
                db:AddItem(p, 2)
            end
        end)
        local crater = Instance.new("Part")
        crater.Size = Vector3.new(10,1,10)
        crater.Anchored = true
        crater.Position = r.Position
        crater.Parent = workspace
        db:AddItem(crater, 3)
    end
    ts:Create(hrp, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {CFrame = b}):Play()
end

