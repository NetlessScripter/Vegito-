local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Assets = ReplicatedStorage:WaitForChild("Assets")
local LocalPlayer = game.Players.LocalPlayer

local function toggleAllEmitters(parent, enabled)
	if not parent then return end
	for _, v in ipairs(parent:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v.Enabled = enabled or false
		end
	end
end

local function doEmitCount(parent)
	if not parent then return end
	for _, v in ipairs(parent:GetDescendants()) do
		if v:IsA("ParticleEmitter") then
			v:Emit(v:GetAttribute("EmitCount") or 0)
		end
	end
end

local function SpawnBlue(char)
	if not char.Parent then return end
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local scaleValue = 0.1
	local clone = Assets.VFX.GojoUlt.MaxBlue:Clone()
	clone.Size *= scaleValue
	clone.Parent = workspace:WaitForChild("VFX")
	clone.Anchored = true
	clone.CanCollide = false
	clone.CFrame = HRP.CFrame * CFrame.new(0, 4.5, -3)
	toggleAllEmitters(clone, true)
	local ObjectValue = Instance.new("ObjectValue")
	ObjectValue.Name = "BlueMax"
	ObjectValue.Value = clone
	ObjectValue.Parent = char
	local startTime = os.clock()
	local conn
	conn = RunService.RenderStepped:Connect(function(dt)
		if char.Parent then
			if os.clock() - startTime <= 5.9 then
				clone.CFrame = clone.CFrame:Lerp(HRP.CFrame * CFrame.new(0, 4.5, math.clamp(-25 * (os.clock() - startTime)/0.3986, -25, 25)), dt * 7.5)
				clone.Size *= scaleValue
			else
				conn:Disconnect()
				Debris:AddItem(clone, 5.9)
				toggleAllEmitters(clone, false)
			end
		end
	end)
end

local function DestroyBlue(char)
	if not char.Parent then return end
	local blue = char:FindFirstChild("BlueMax")
	if blue and blue.Value then
		local val = blue.Value
		blue:Destroy()
		Debris:AddItem(val, 1.5)
		toggleAllEmitters(val, false)
	end
end

local function FinisherBlue(char, targetCFrame)
	if not char.Parent or not targetCFrame then return end
	local blue = char:FindFirstChild("BlueMax")
	if blue and blue.Value then
		local val = blue.Value
		blue:Destroy()
		local startTime = os.clock()
		local conn
		conn = RunService.RenderStepped:Connect(function(dt)
			if os.clock() - startTime <= 3.5 then
				val.CFrame = val.CFrame:Lerp(targetCFrame, dt * 7)
			else
				Debris:AddItem(val, 1.5)
				toggleAllEmitters(val, false)
				conn:Disconnect()
			end
		end)
	end
end

local function FinisherCharacter(char, targetChar)
	if not char.Parent or not targetChar.Parent then return end
	local blue = char:FindFirstChild("BlueMax")
	local targetHRP = targetChar:FindFirstChild("HumanoidRootPart")
	if blue and blue.Value and targetHRP then
		for _, v in ipairs(targetChar:GetDescendants()) do
			if v:IsA("Part") or v:IsA("BasePart") or v:IsA("MeshPart") or v:IsA("Decal") then
				TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Linear), {Transparency = 1}):Play()
			end
		end
		local CFrame_upvw = blue.Value.CFrame
		local tbl = {}
		for i=1,25 do
			local clone = script:FindFirstChild("BloodParticle"):Clone()
			clone.Parent = workspace.VFX
			clone.CFrame = CFrame_upvw
			table.insert(tbl, clone)
			local body = Instance.new("BodyVelocity")
			body.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
			body.P = math.huge
			body.Velocity = Vector3.new(math.random(-8,8),math.random(8,11),math.random(-8,8))
			body.Parent = clone
			Debris:AddItem(body, 0.1)
			local att = Instance.new("Attachment", clone)
			local align = Instance.new("AlignPosition", clone)
			align.Attachment0 = att
			align.Mode = Enum.PositionAlignmentMode.OneAttachment
			align.RigidityEnabled = true
		end
		local conn
		conn = RunService.Heartbeat:Connect(function()
			for _, v in ipairs(tbl) do
				v:Destroy()
			end
			conn:Disconnect()
		end)
	end
end

local function startEffects(char, arg2)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	for _, v in ipairs(Assets.VFX.Gojo.BlueMax.WindupHRP:GetChildren()) do
		if v:IsA("Attachment") then
			local clone = v:Clone()
			clone.Parent = HRP
			toggleAllEmitters(clone, true)
			Debris:AddItem(clone, 10)
		end
	end
	if arg2 then
		local tbl = {}
		local function stopAll()
			for _, v in ipairs(tbl) do
				toggleAllEmitters(v, false)
			end
		end
	end
end

local function newBlueSpawn(char)
	local HRP = char:FindFirstChild("HumanoidRootPart")
	if not HRP then return end
	local clone = Assets.VFX.Gojo.BlueMax.BlueMax:Clone()
	clone.CFrame = HRP.CFrame
	clone.Anchored = true
	clone.Parent = workspace.VFX
	toggleAllEmitters(clone, true)
	Debris:AddItem(clone, 10)
	local bool = Instance.new("BoolValue")
	bool.Name = "maxblue"
	bool.Value = true
	bool.Parent = char
	Debris:AddItem(bool, 6)
	local clone2 = Assets.VFX.Gojo.BlueMax.ActiveHRP.Ground:Clone()
	clone2.Parent = HRP
	toggleAllEmitters(clone2, true)
	Debris:AddItem(clone2, 10)
	local function moveClone()
		if char.Parent and bool.Parent then
			TweenService:Create(clone, TweenInfo.new(0.5), {CFrame = HRP.CFrame * CFrame.new(0,4.5,-25)}):Play()
		else
			toggleAllEmitters(clone, false)
			toggleAllEmitters(clone2, false)
		end
	end
end

local function endBlue(char)
	local bool = char:FindFirstChild("maxblue")
	if bool then
		bool:Destroy()
	end
end


