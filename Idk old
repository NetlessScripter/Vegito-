game.Players.LocalPlayer.Character:WaitForChild("Humanoid").AnimationPlayed:Connect(onAnimationPlayed)

local targetAnimID = "rbxassetid://12307656616"
local isPlaying = false

local function onAnimationPlayed(animationTrack)
    if isPlaying then return end
    if animationTrack.Animation.AnimationId == targetAnimID then
        isPlaying = true
        animationTrack:Stop()

        local Players = game:GetService("Players")
        local Player = Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local Humanoid = Character:WaitForChild("Humanoid")

        local RisingFistAnim = Instance.new("Animation")
        RisingFistAnim.AnimationId = "rbxassetid://18896127525"
        local AnimTrack = Humanoid:LoadAnimation(RisingFistAnim)
        AnimTrack:Play()
        task.delay(0.5, function()
            AnimTrack:Stop()
        end)

        local Test = game.ReplicatedStorage.Assets.VFX.YujiWallCombo["Right Arm"]["2"]
        local test = Test:Clone()
        local hrp = Character:WaitForChild("Right Arm")
        test.Parent = hrp

        for _, child in ipairs(test:GetChildren()) do
            if child:IsA("ParticleEmitter") then
                child.Enabled = true
                child:Emit(1)
            end
        end
task.spawn(function()
wait(0.5)
test:Destroy()
end)
        task.spawn(function()
            wait(0.5)
            isPlaying = false
        end)
    end
end

