local targetAnimID = "rbxassetid://10466974800"
local newAnimID = "rbxassetid://12273188754"

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")

local function playNewAnimation()
    local anim = Instance.new("Animation")
    anim.AnimationId = newAnimID
    local track = Humanoid:LoadAnimation(anim)
    track:Play()
end

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == targetAnimID then

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")
local TweenService = game:GetService("TweenService")
local LocalPlayer = game.Players.LocalPlayer

local Brutal_Beatdown = ReplicatedStorage:WaitForChild("Assets").VFX.MetalBat["Brutal Beatdown"]

local function Clear(parent, className)
	for _, v in parent:GetDescendants() do
		if v:IsA(className) then
			v:Destroy()
		end
	end
end

local function Hit1Mesh(part)
	local clone = Brutal_Beatdown.Meshes.Hit:Clone()
	Debris:AddItem(clone, 2.75)
	for _, v in pairs(clone:GetChildren()) do
		v.Parent = workspace.VFX
		v:PivotTo(part:GetPivot())
		Debris:AddItem(v, 2.75)
	end
end

local function SlamMesh(part)
	local clone = Brutal_Beatdown.Meshes.Hit:Clone()
	Debris:AddItem(clone, 2.75)
	for _, v in pairs(clone:GetChildren()) do
		v.Parent = workspace.VFX
		v:PivotTo(part:GetPivot() * CFrame.new(0, 0, -3))
		Debris:AddItem(v, 2.75)
	end
end

local function RootPush(rootPart, name)
	Clear(rootPart, "BodyVelocity")
	local velocityValue = Instance.new("NumberValue")
	velocityValue.Value = 80

	local bodyVelocity = Instance.new("BodyVelocity")
	bodyVelocity.Name = name or "Brutal Beatdown Velocity Push Force"
	bodyVelocity.Velocity = rootPart.CFrame.LookVector * velocityValue.Value
	bodyVelocity.MaxForce = Vector3.new(8999999, 0, 8999999)
	bodyVelocity.Parent = rootPart

	task.delay(0.4, bodyVelocity.Destroy, bodyVelocity)
	TweenService:Create(velocityValue, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		Value = 0
	}):Play()

	while bodyVelocity:IsDescendantOf(workspace) and rootPart:IsDescendantOf(game) do
		bodyVelocity.Velocity = rootPart.CFrame.LookVector * velocityValue.Value
		task.wait()
	end

	if bodyVelocity then
		bodyVelocity:Destroy()
	end
end

local function BrutalBeatdownSwing(character)
	if not character or not character.Parent then return end
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local clone = Brutal_Beatdown.BrutalBeatdownSwing:Clone()
	clone.CFrame = hrp.CFrame
	clone.Weld.Part0 = hrp
	clone.Parent = hrp

	for _, v in clone:GetDescendants() do
		if v:IsA("ParticleEmitter") then
			local count = v:GetAttribute("EmitCount")
			task.delay(v:GetAttribute("EmitDelay") or 0, function()
				v:Emit(count)
				v.LockedToPart = true
				task.wait(0.1)
				v.LockedToPart = false
			end)
		end
	end

	task.delay(4, clone.Destroy, clone)
end

local function BrutalBeatdownSlam(character)
	if not character or not character.Parent then return end
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local clone = Brutal_Beatdown.BrutalBeatdownSlam:Clone()
	clone.CFrame = hrp.CFrame
	clone.Weld.Part0 = hrp
	clone.Parent = hrp

	for _, v in clone:GetDescendants() do
		if v:IsA("ParticleEmitter") then
			local count = v:GetAttribute("EmitCount")
			task.delay(v:GetAttribute("EmitDelay") or 0, function()
				v:Emit(count)
				v.LockedToPart = true
				task.wait(0.1)
				v.LockedToPart = false
			end)
		end
	end

	task.delay(4, clone.Destroy, clone)
end

local function BrutalBeatdownHit1(character)
	if not character or not character.Parent then return end
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local clone = Brutal_Beatdown.BrutalBeatdownHit1:Clone()
	clone.Parent = hrp
	clone.CFrame = hrp.CFrame
	clone.Weld.Part0 = hrp

	for _, v in clone:GetDescendants() do
		if v:IsA("ParticleEmitter") then
			local count = v:GetAttribute("EmitCount")
			task.delay(v:GetAttribute("EmitDelay") or 0, function()
				v:Emit(count)
				v.LockedToPart = true
				v.TimeScale = 1
				task.wait(0.025)
				v.TimeScale = 0.025
				task.wait(0.1)
				v.TimeScale = 1
				v.LockedToPart = false
			end)
		end
	end

	task.delay(4, clone.Destroy, clone)
end

local function BrutalBeatdownHit2(character)
	if not character or not character.Parent then return end
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local clone = Brutal_Beatdown.BrutalBeatdownHit2:Clone()
	clone.CFrame = hrp.CFrame
	clone.Weld.Part0 = hrp
	clone.Parent = hrp

	for _, v in clone:GetDescendants() do
		if v:IsA("ParticleEmitter") then
			local count = v:GetAttribute("EmitCount")
			task.delay(v:GetAttribute("EmitDelay") or 0, function()
				v:Emit(count)
				v.LockedToPart = true
				task.wait(0.1)
				v.LockedToPart = false
			end)
		end
	end

	task.delay(4, clone.Destroy, clone)
end

        animationTrack:Stop()
        playNewAnimation()

