local Debris = game:GetService("Debris")
local LocalPlayer = game.Players.LocalPlayer
local Impact = game:GetService("ReplicatedStorage").Replication.GojoAdmin["Reversal Red Max"].Impact

local function ImpactFrames()
	local clone = Impact:Clone()
	clone.Parent = LocalPlayer:WaitForChild("PlayerGui")
	local images = {}
	for _, obj in ipairs(clone:GetDescendants()) do
		if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
			obj.Visible = false
			table.insert(images, obj)
		end
	end

	for _, img in ipairs(images) do
		img.Visible = true
		task.wait(0.03)
		img.Visible = false
	end

	Debris:AddItem(clone, 3)
end

ImpactFrames()
end)
